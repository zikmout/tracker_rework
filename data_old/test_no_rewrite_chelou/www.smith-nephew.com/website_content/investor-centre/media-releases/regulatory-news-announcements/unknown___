                if ((url.indexOf("fi/finland") != -1) || (url.indexOf("no/norway") != -1) || (url.indexOf("de/austria") != -1) || (url.indexOf("it/italy") != -1) || (url.indexOf("fr/france") != -1) || (url.indexOf("fr-be/belgium") != -1) || (url.indexOf("en-gb/uk") != -1) || (url.indexOf("sv/sweden") != -1) || (url.indexOf("da/denmark") != -1) || (url.indexOf("es/spain") != -1) || (url.indexOf("nl-be/belgium") != -1) || (url.indexOf("nl/netherlands") != -1)) {
