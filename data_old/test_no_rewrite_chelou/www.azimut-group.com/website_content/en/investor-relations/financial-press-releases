						<h4 class="panel-title title-blue">
							























































 Media<a id="accordion2" class="az-accordion-right collapsed" onclick="openAccordion('2');"></a>
						</h4>
					</div>
					<script type="text/javascript">
					    function openAccordion(idRow){					    	
					    	if($('#collapse'+idRow).hasClass('in')){
					    		$('#collapse'+idRow).removeClass('in');	
					    		$('#accordion'+idRow).addClass('collapsed');
					    	}else{
					    		$('#collapse'+idRow).addClass('in');
					    		$('#accordion'+idRow).removeClass('collapsed');
					    		
					    	}					    	
					    }
					</script>			
					
