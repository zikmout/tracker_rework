<script type="text/javascript">
	/*POPOVER COMPARTIR EN REDES*/
	$(document).ready(function(){
		$('.ico-redes, .popover').hover(function(){
			$('.popover').show();
		},function(){
			$('.popover').hide();
		});
	});

	$(document).ready(function(){
		var width = $(window).width();
		if ((width <= 992)) {
			$('html').on('mouseup', function(e) {
				if(!$(e.target).closest('.popover').length) {
					$('.popover').each(function(){
						$(this.previousSibling).popover('hide');
					});
				}
			})
		} 
	});
	
	/*COMPARTIR EN FACEBOOK*/
	function compartirFacebook()
	{
		var paginaActual = window.location;
		var urlFacebook = "http://www.facebook.com/sharer.php?u=" + encodeURIComponent(paginaActual);
		window.open(urlFacebook, "", "menubar=no, toolbar=no, resizable=yes, scrollbars=yes, height=600, width=600");
	}
	
	/*COMPARTIR EN TWITTER*/
	function compartirTwitter()
	{
		var paginaActual = window.location;
		var urlTwitter = "http://twitter.com/share?url=" + encodeURIComponent(paginaActual);
		window.open(urlTwitter, "", "menubar=no, toolbar=no, resizable=yes, scrollbars=yes, height=600, width=600");
	}
	
	/*COMPARTIR EN LINKEDIN*/
	function compartirLinkedIn()
	{
		var paginaActual = window.location;
		var urlLinkedIn = "http://www.linkedin.com/shareArticle?mini=true&url=" + encodeURIComponent(paginaActual);
		window.open(urlLinkedIn, "", "menubar=no, toolbar=no, resizable=yes, scrollbars=yes, height=600, width=600");
	}
	
	/*COMPARTIR EN GOOGLE PLUS*/
	function compartirGooglePlus()
	{
		var paginaActual = window.location;
		var urlGooglePlus = "http://plus.google.com/share?url=" + encodeURIComponent(paginaActual);
		window.open(urlGooglePlus, "", "menubar=no, toolbar=no, resizable=yes, scrollbars=yes, height=600, width=600");
	}
	
	/*COMPARTIR POR CORREO*/
	$(document).ready(function(){
		var enlace = "mailto:?subject="+document.title+"&body="+window.location;
		var emailLink = document.getElementById('emailLink');
		emailLink.href = enlace;
	});
	
</script>

