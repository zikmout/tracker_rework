<script>
    $(document).ready(function () {
        $('a').each(function () {
            var a = new RegExp('www.cnmv.es/portal/home.aspx?lang=en');
            if (a.test(this.href)) {
                $(this).attr("target", "_blank");
            }
        });
    });
</script>
<script>
    function openFancyboxConfirmation(origin) {
        var ebookhref = "#message-confirmation-box";
        if (origin == "ebook") { ebookhref = "#message-confirmation-box-ebook" }
        $.fancybox({
            'href': ebookhref,
            'width': "680px",
            'wrapCSS': "fancybox-newsletter"
        });

    }
    function openFancyboxExistentUser() {
        $.fancybox({
            'href': '#message-confirmation-box-existent',
            'width': "680px",
            'wrapCSS': "fancybox-newsletter"
        });

    }

    function openFancyboxError() {
        $.fancybox({
            'href': '#message-confirmation-box-error',
            'width': "680px",
            'wrapCSS': "fancybox-newsletter"
        });
    }

    function openFancyboxConfirmationUnsubs() {
        $.fancybox({
            'href': '#message-confirmation-unsubscribe',
            'width': "680px",
            'wrapCSS': "fancybox-newsletter"
        });
    }

</script>
