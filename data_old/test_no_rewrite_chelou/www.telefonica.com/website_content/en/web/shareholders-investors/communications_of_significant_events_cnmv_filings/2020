		position: relative;
		-webkit-transition-property: color;
		transition-property: color;
		-webkit-transition-property: background-color;
		transition-property: background-color;	
		-webkit-transition-duration: 0.3s;
		transition-duration: 0.3s;	
	} 
	.mod_hechos .hch-listado .hch-item a.hch-link:hover { 
		text-decoration: none; 
		background-color: #00bfd6;
		color: #ffffff;	
		-webkit-transition: background-color 0.3s ease-out;
		-moz-transition: background-color 0.3s ease-out;
		-o-transition: background-color 0.3s ease-out;
		transition: background-color 0.3s ease-out;
	}
	.mod_hechos .hch-listado .hch-item a.hch-link:active, .mod_hechos .hch-listado .hch-item a.hch-link:visited { 
		text-decoration: none; 
	}

/* MAX-WIDTH : 668PX */
@media (max-width:668px) {

	.mod_hechos .hch-listado .hch-item {
		padding-bottom: 1.8em;
		padding-left: 1em;
		background: url(/telefonica-theme/images/dFront/modules/icons/blt_relacionados.gif) no-repeat left 0.4em;
	}

	.mod_hechos .hch-listado .hch-item .hch-date {
		font-size: 1em;
		line-height: 1em;

	}
	.mod_hechos .hch-listado .hch-item .hch-title {
		font-size: 1.3em;
		line-height: 1em;
	}

	.mod_hechos .hch-listado .hch-item a.hch-link {
		font-size: 0.9em;
	}

}	

</style>
			
			
		


	
	





	

					
				
			
		
	
	



	</div>

				</div>

				
			
		
	


	







	</div>




</div>
			</div>
		</div>
