       }

        function BuscarCabecera() {

            var textoBusqueda = document.getElementById(idInputSearch);


            if (textoBusqueda != null) {

                var valorQuery = "q=".concat(encodeURIComponent(textoBusqueda.value));
                /*
                -- viejo buscador
                var valorSort = "&sort=".concat(encodeURIComponent("meta:relevance"));
