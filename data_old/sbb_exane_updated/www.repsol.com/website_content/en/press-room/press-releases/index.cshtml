      function runScript(e) {
           if (e.which == 13 || event.keyCode == 13) {
               BuscarCabecera();
           }
       }

        function BuscarCabecera() {

            var textoBusqueda = document.getElementById(idInputSearch);


            if (textoBusqueda != null) {

                var valorQuery = "q=".concat(encodeURIComponent(textoBusqueda.value));
                /*
                -- viejo buscador
                var valorSort = "&sort=".concat(encodeURIComponent("meta:relevance"));
                var valorGetFields = "&getfields=".concat(encodeURIComponent("*"));
                var valorOutput = "&output=".concat(encodeURIComponent("xml_no_dtd"));
                var valorClient = "&client=".concat(encodeURIComponent("front_colecciones"));
                var valorSite = "&site=".concat(encodeURIComponent(ParseSites()));
                var valorSinFiltro = "&filter=0";
                */

                window.location.href = '/en/buscador.cshtml'.concat("?").concat(valorQuery);//.concat(valorSort).concat(valorGetFields).concat(valorOutput).concat(valorClient).concat(valorSite).concat(valorSinFiltro);
            } else {

                   window.location.href = '/en/buscador.cshtml';
            }

        }

    function ParseSites() {
        var queryHeader = JSON.parse(coleccionesGsa);
        var language = $('html')[0].lang;
        var querySite = "";
        for (var i = 0; i < queryHeader.coleccionesGsa.length; i++) {
            var querySiteAux = "";
            for (var j = 0; j < queryHeader.coleccionesGsa[i].idiomas.length; j++) {
                if (queryHeader.coleccionesGsa[i].idiomas[j].idioma == language) {
                    querySiteAux = queryHeader.coleccionesGsa[i].idiomas[j].coleccion;
                }
            }
            if (!querySiteAux) {
                querySiteAux = queryHeader.coleccionesGsa[i].coleccionPorDefecto;
            }
            if (!querySite) {
                querySite = querySiteAux;
            } else {
                querySite = querySite.concat("|").concat(querySiteAux);
            }
        }
        return querySite;
    }



    </script>
 
