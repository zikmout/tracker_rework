    <script type='text/javascript'>
        
        var chartbeatSection = "actualite";
        var chartbeatSousSection = "points-de-marche";

        if(chartbeatSousSection == 'toute-l-actualite' || chartbeatSousSection == 'tous-les-conseils-bourse' || chartbeatSousSection == 'produit-derive' || chartbeatSousSection == 'produit-derive' ){
            chartbeatSection = 'home ' + chartbeatSection;
        }else if(chartbeatSection == 'accueil'){
            if(window.location.href.indexOf("forex") > -1) {
                chartbeatSection = 'home forex';
            }else{
                chartbeatSection = 'home';
            }
        }else if((chartbeatSection == 'outils-de-trading' && chartbeatSousSection == '') || (chartbeatSection == 'membres' && chartbeatSousSection == '') || (chartbeatSection == 'placement' && chartbeatSousSection == '') || (chartbeatSection == 'apprendre-la-bourse' && chartbeatSousSection == 'index')){
            chartbeatSection = 'home ' + chartbeatSection;
        }else if(chartbeatSousSection == 'portefeuille-trading'){
            chartbeatSection = 'home ' + chartbeatSousSection;
        }else if(chartbeatSousSection == 'actualites' || chartbeatSection == 'actualite') {
            chartbeatSection = 'actualite';
        }else{
            chartbeatSection = chartbeatSousSection;
        }

        (function() {
            /** CONFIGURATION START **/
            var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});

            _sf_async_config.uid = 121042;
            _sf_async_config.domain = 'tradingsat.com';
            _sf_async_config.flickerControl = false;
            _sf_async_config.useCanonical = true;
            _sf_async_config.useCanonicalDomain = true;
            _sf_async_config.sections = chartbeatSection;
            _sf_async_config.authors = '';
            /** CONFIGURATION END **/
            function loadChartbeat() {
                var e = document.createElement('script');
                var n = document.getElementsByTagName('script')[0];
                e.type = 'text/javascript';
                e.async = true;
                e.src = '//static.chartbeat.com/js/chartbeat.js';
                n.parentNode.insertBefore(e, n);
            }
            loadChartbeat();
        })();
    </script>


    </script>

