        <div id="find-panel-wrapper" class="gs-search hide">
                <div id="find-panel-search-box" class="search-box">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-lg-12 col-xs-12 wrapper">
                        <div action="/en/investor-relations/search-page2/">
                            <div class="form-group form-group-lg pad-t-20">
                                <div class="input-group col-xs-12 search-btn">
                                    <input id="findInputHeader" type="text" class="form-control" autofocus />
                                    <div id="findInputButton" class="input-group-addon">
                                        Search
                                    </div>
                                </div>
                            </div>
                        </div>
                </div>
            </div>
            <div class="row pad-t-20 pad-b-20">
                    <div class="col-md-2">
                        <img class="search-lens" src="/modules/digitalfirsttemplates/images/schroders-icons/lens_thick_dark.svg" />
                    </div>
                <!-- PROMOTED SEARCH>> Only if we are not on find page and we have related search -->
                <div class="col-md-10 wrapper" id="find-header-promoted-links">
                        <div class="row">
                            <div class="col-md-12">
                                <h3>Are you looking for...</h3>
                            </div>
                        </div>
                        <div class="row pad-t-10">
                                <div class="col-md-6 promoted-links">
                                    <a href="/en/investor-relations/results-and-reports/results-reports-and-presentations/">Results, reports and presentations</a>
                                </div>
                                <div class="col-md-6 promoted-links">
                                    <a href="/en/investor-relations/results-and-reports/ir-calendar/">IR calendar</a>
                                </div>
                                <div class="col-md-6 promoted-links">
                                    <a href="/en/investor-relations/results-and-reports/annual-report-and-accounts-2016/">Annual Report and Accounts 2016</a>
                                </div>
                                <div class="col-md-6 promoted-links">
                                    <a href="/en/investor-relations/regulatory-news/newsfeed/">Newsfeed</a>
                                </div>
                        </div>

                </div>
                <div class="col-md-10 wrapper" id="find-header-suggested-links" style="display: none">
                    <div class="row">
                        <div class="col-md-12">
                            <h3>Related Search</h3>
                        </div>
                    </div>
                    <div class="row pad-t-10" id="find-header-suggested-links-render">

                    </div>
                </div>
            </div>
        </div>
    </div>

<script>
var FIND_AUDIENCES_IDS = [208597];
var FIND_EXCLUDE_PARENT_IDS = [];


document.addEventListener("DOMContentLoaded",
    function() {

        var FIND_INPUT_ID = 'find-panel-search-box';

        //if (bowser.mobile || bowser.tablet) {
        //    document.getElementById(FIND_INPUT_ID).classList.add('mobile');
        //}
        var WINDOW_ORIGIN = window.location.protocol +
            "//" +
            window.location.hostname +
            (window.location.port ? ':' + window.location.port : '');
        var CURRENT_LANGUAGE_CODE = $('body').attr('data-find-language-code');
        var RELATED_SEARCH_ENDPOINT = WINDOW_ORIGIN + '/' + CURRENT_LANGUAGE_CODE + '/api/findapi/relatedsearch';
        var TIMER_DEBOUNCE = 500;
        var FIND_INPUT_ID = '#findInputHeader';
        var FIND_INPUT_BUTTON = '#findInputButton';
        var HEADER_PROMOTED_LINKS = '#find-header-promoted-links';
        var HEADER_SUGGESTED_LINKS = '#find-header-suggested-links';
        var HEADER_SUGGESTED_LINKS_RENDER = '#find-header-suggested-links-render';
        var FIND_PAGE_URL = '/en/investor-relations/search-page2/';
        var HTML_TO_APPEND = '';
        $(HEADER_SUGGESTED_LINKS).hide();
        var QUERY_OBJECT = {
            KeyWords: '',
            Skip: 0,
            Take: 12,
            ResultTypeFacets: [],
            FilterFacets: [],
            SortType: 'Relevant'
        };

        $(FIND_INPUT_ID)
            .keyup(function(e) {
                clearTimeout($.data(this, 'timer'));
                if (e.keyCode === 13) {
                    submitFindInput();
                } else {
                    $(this).data('timer', setTimeout(search, TIMER_DEBOUNCE));
                }
            });


        $(FIND_INPUT_BUTTON).on('click',
            function() {
                submitFindInput();
            });

        function submitFindInput() {
            var keyWord = $(FIND_INPUT_ID).val();
            navigateToFindPage(keyWord);
        }


        function navigateToFindPage(keyWord) {
            window.location.href = window.location.origin + FIND_PAGE_URL + '#' + keyWord;
        }

        function search(force) {
            var keyWord = $(FIND_INPUT_ID).val();
            if (!force && keyWord.length < 3) return;
            QUERY_OBJECT.KeyWords = keyWord;
            QUERY_OBJECT.AudienceFacets = [];
            FIND_AUDIENCES_IDS.forEach(function(item) {
                QUERY_OBJECT.AudienceFacets.push({ AudienceId: item });
            });

            QUERY_OBJECT.ExcludeParent = FIND_EXCLUDE_PARENT_IDS;

            $.ajax({
                type: "POST",
                url: RELATED_SEARCH_ENDPOINT,
                data: QUERY_OBJECT,
                success: function(result) {

                    if (result.TotalResults > 0) {
                        $(HEADER_PROMOTED_LINKS).hide();
                        //render  HEADER_SUGGESTED_LINKS
                        HTML_TO_APPEND = '';
                        result.SearchResultHits.forEach(function(item) {
                            HTML_TO_APPEND = HTML_TO_APPEND.concat('<div class="col-md-6 pad-t-10">');
                            HTML_TO_APPEND = HTML_TO_APPEND
                                .concat('<a href="' +
                                    item.HitUrl +
                                    '" target="_blank" class="suggestion">' +
                                    item.HitTitle +
                                    '</a>');
                            HTML_TO_APPEND = HTML_TO_APPEND.concat('</div>');
                        });
                        $(HEADER_SUGGESTED_LINKS_RENDER).html(HTML_TO_APPEND);
                        $(HEADER_SUGGESTED_LINKS).show();
                    }
                },
                error: function(XMLHttpRequest) {
                    console.log('error>>', XMLHttpRequest.status, XMLHttpRequest.statusText);
                }
            });
        }
    });
</script>


        </div>
</div>
<script>
    var FIND_SEARCH_ICON = '#gsSearchIcon';
    var FIND_PANEL_WRAPPER = '#find-panel-wrapper';
    var IS_FIND_PAGE = 'False';
    var IS_MOBILE_ONLY = '.mobile-only';
    var AUTHENTICATION_SIGN_UP = '.pfpc-sign-up';
    var AUTHENTICATION_SIGN_UP_PAGE = '#authentication-sign-up';
    var AUTHENTICATION_SIGN_UP_PAGE_CLOSE = '#authentication-sign-up a.close-filter';
    var AUTHENTICATION_SIGN_UP_DESKTOP_LINK = 'a.pfpc-sign-up';
    var AUTHENTICATION_SIGN_IN = '.pfpc-sign-in';
    var AUTHENTICATION_SIGN_IN_PAGE = '#authentication-sign-in';
    var AUTHENTICATION_SIGN_IN_PAGE_CLOSE = '#authentication-sign-in a.close-filter';
    var AUTHENTICATION_SIGN_IN_DESKTOP_LINK = 'a.pfpc-sign-in';
    var AUTHENTICATION_LOGOUT = '.pfpc-user a.logout';
    var AUTHENTICATION_MOBILE_SIGN_UP = 'a.mobile-sign-up';
    var AUTHENTICATION_MOBILE_SIGN_IN = 'a.mobile-sign-in';

    function insertParam(url, key, value) {
        var aMock = document.createElement('a');
        aMock.href = url;
        var values = aMock.search;
        var encodedKey = encodeURI(key);
        var encodedValue = encodeURI(value);
        var kvp = values.substr(1).split('&');
        var i = kvp.length;

        while (i--) {
            var x = kvp[i].split('=');

            if (x[0] === encodedKey) {
                x[1] = encodedValue;
                kvp[i] = x.join('=');
                break;
            }
        }

        if (i < 0) {
            kvp[kvp.length] = [encodedKey, encodedValue].join('=');
        }

        aMock.search = kvp.join('&');

        return aMock.href;
    }

    function createNewEvent(eventName) {
        if (typeof (Event) === 'function') {
            return new Event(eventName);
        } else {
            var event = document.createEvent("Event");
            event.initEvent(eventName, false, true);
            return event;
        }
    }

    //gsSearchIcon
    document.addEventListener("DOMContentLoaded", function () {
        $(FIND_SEARCH_ICON).on('click', function () {
            if (!$(this).find(IS_MOBILE_ONLY).is(":visible") && IS_FIND_PAGE !== 'True') {
                $(this).toggleClass('active');
            }

            $(FIND_PANEL_WRAPPER).toggleClass('hide');

            if ($(FIND_PANEL_WRAPPER).hasClass("hide")) {
                document.dispatchEvent(createNewEvent("search-closed"));
            } else {
                document.dispatchEvent(createNewEvent("search-opened"));
            }
        });
        if (IS_FIND_PAGE === 'True') {
            $(FIND_SEARCH_ICON).addClass('active');

        }
        if (AUTHENTICATION_SIGN_UP.length) {

            $(AUTHENTICATION_SIGN_UP).on('hidden.bs.dropdown shown.bs.dropdown',
                function () {
                    $(AUTHENTICATION_SIGN_UP_PAGE).toggleClass('open');
                });

            $(AUTHENTICATION_SIGN_UP_PAGE_CLOSE).on('click keypress', function () {
                $(AUTHENTICATION_SIGN_UP).toggleClass('open');
                $(AUTHENTICATION_SIGN_UP_PAGE).toggleClass('open');
            });

            $(AUTHENTICATION_SIGN_UP_PAGE).on('click keypress', function (event) {
                event.stopPropagation();
            });

            $(AUTHENTICATION_MOBILE_SIGN_UP).on('click keypress',
                function(event) {
                    event.stopPropagation();
                    $(AUTHENTICATION_SIGN_UP_DESKTOP_LINK).click();
                });
        }
        if (AUTHENTICATION_SIGN_IN.length) {

            $(AUTHENTICATION_SIGN_IN).on('hidden.bs.dropdown shown.bs.dropdown',
                function() {
                    $(AUTHENTICATION_SIGN_IN_PAGE).toggleClass('open');
                });

            $(AUTHENTICATION_SIGN_IN_PAGE_CLOSE).on('click keypress', function () {
                $(AUTHENTICATION_SIGN_IN).toggleClass('open');
                $(AUTHENTICATION_SIGN_IN_PAGE).toggleClass('open');
            });

            $(AUTHENTICATION_SIGN_IN_PAGE).on('click keypress', function (event) {
                event.stopPropagation();
            });

            $(AUTHENTICATION_MOBILE_SIGN_IN).on('click keypress',
                function (event) {
                    event.stopPropagation();
                    $(AUTHENTICATION_SIGN_IN_DESKTOP_LINK).click();
                });
        }
        $(AUTHENTICATION_LOGOUT).on('click',
            function() {
                var pageID = 233972,
                    lang = 'en',
                    requestVerificationToken = 'Yv2WgBDoEFe9OqL7O-zJfk1XFVKlZ620vzkmWBW09HO8Xp62HUAve7rS_eZu8M-mp5u5HpQ8vuZoKVVrd2z95LkzxWm9z_k-hZsF0ZiZxQ01:bbB5JpJm86c_9R4P2pGeeMLzCMBL7SfVul9h1xsXr9OXoHlphqXANfIYlbhwGCcsSlcfPiGM3IsNRpYprGgrNoD20W78Jv-UETZa96pgIkI1',
                    api = '/authentication/logout',
                    endPoint = '';

                endPoint = endPoint.concat('/', lang, api);

                $.ajax({
                    url: endPoint,
                    data: { pageId: pageID },
                    type: "POST",
                    context: this,
                    dataType: "json",
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader('requestVerificationToken', requestVerificationToken);
                    },
                    success: function (result) {
                        if (result.RedirectUrl) {
                            var cacheParam = encodeURIComponent(btoa(Math.random().toPrecision(6)));

                            window.location.href = insertParam(result.RedirectUrl, 'cache', cacheParam);
                        } else {
                            window.location.reload(true);
                        }
                    }
                });
            });
    });
</script>



