        // It is recommended to always add the [1, 1] size first
            .defineSlot("/162547629/latribune.fr/home//native", [[1, 1], "fluid"], "div-gpt-ad-home-native-0")
            .defineSizeMapping(
                googletag.sizeMapping()
                    .addSize([1000, 0], [[1, 1], "fluid"])
                    .addSize([0, 0], [])
                    .build()
            )
            .setForceSafeFrame(true)
            .setSafeFrameConfig({ allowPushExpansion: true, sandbox: true })
            .addService(googletag.pubads());

        // force SafeFrames on tablets and phones
        if (matchMedia("(max-width: 729.98px)").matches) {
            googletag.pubads().setForceSafeFrame(true);
            googletag.pubads().setSafeFrameConfig({ allowPushExpansion: true, sandbox: true });
        }

        // Google must be configured with SRA
        googletag.pubads().enableSingleRequest();

        // Recommended
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
    });
</script>

    


