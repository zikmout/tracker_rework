<script>
    $(document).ready(function() {

        $('.popup-listeaction').click(function (e) {
            e.preventDefault();
            var width = $(window).width();
            var  height = $(window).height();
            var title = $(this).attr('title');
            $.ajax({
                url: $(this).attr('href'),
                success: function (response) {
                    $('#mask').css('display','block');
                    $('#mask').css('position','fixed');
                    $('#mask').css('top','0px');
                    $('#mask').css('left','0px');
                    $('#mask').css('height','100%');
                    $('#mask').css('width','100%');
                    $('#mask').css('z-index', '100002');
                    $('#mask').css('background-color', '#000');
                    $('#mask').css('opacity', '0.75');

                    $('#ajouter').html(response);

                    $('#ajouter').css('border','10px solid #ffffff;');
                    $('#ajouter').css('position','fixed');
                    $('#ajouter').css('background','#ffffff');
                    $('#ajouter').css('color','#000000');

                    $('#ajouter').css('z-index', '100003');
                    if (width >= 1000 ){
                        $('#ajouter').css('top','30%');
                        $('#ajouter').css('left','35%');
                    }else if(width >= 768){
                        $('#ajouter').css('top','20%');
                        $('#ajouter').css('left','26%');
                    }else{
                        $('#ajouter').css('top','5%');
                        $('#ajouter').css('left','0%');
                    }
                    $('#ajouter').css('display','block');
                    $('#ajouter .title-popup').append(title);

                },error: function (err) {
                   console.log(err);
                }

            });
        });

        $('.fb_twitter').click(function (e) {
            e.preventDefault();
            window.open(this.href,'Partage', 'height=400 , width=400 , location=no , resizable=yes , scrollbars=yes');
        });

    });


</script>
	
	
