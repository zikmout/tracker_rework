                var queryParams = window.location.href.split('?')[1];
                if (queryParams) {
                    queryParams = queryParams.split('&');
                    queryParams.forEach(function (p) {
                        var param = p.split('=');
                        if (param[0] === '20minutest') {
                            googletag.pubads().setTargeting(param[0], param[1]);
                        }
                    });
                }
            } catch(e) {}

                            const dfpTrackingConsentValue = tC.getCookie("euconsent") === '' ? 1 : 0;

                googletag.pubads().setRequestNonPersonalizedAds(dfpTrackingConsentValue);
                googletag.pubads().setTargeting('consent', '' + dfpTrackingConsentValue);
            
            googletag.pubads().enableSingleRequest();
            googletag.pubads().disableInitialLoad();
            googletag.pubads().collapseEmptyDivs();
            googletag.enableServices();


            var width = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
            if (width >= 1000) {
                googletag.pubads().setTargeting('screen', '1000');
            }
            if (width >= 1400) {
                googletag.pubads().setTargeting('screen', ['1400', '1000']);
            }
        });
    }(window.googletag))
