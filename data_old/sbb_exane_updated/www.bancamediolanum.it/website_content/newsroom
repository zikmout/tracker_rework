    if (jQuery(".check_BoxSlider").length > 0 && switchBoxSlider != undefined) {
        document.write('<script src="/static-assets/js/vendor/plugins/jquery.swipe.js"><\/script>');
        document.write('<script src="/static-assets/js/plugins/bm-slider.js"><\/script>');
    }
