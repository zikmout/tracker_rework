<script type="text/javascript">
    $('.sideNavPicture').find('.illus').each(function () {
        if ((this.className + ' ').indexOf('loaded') < 0) {
            //console.log('image ko => force');
            this.setAttribute('src', this.getAttribute('data-original'));
            this.className = this.className + ' loaded';
        }
        if (this.getAttribute('src') === null || this.getAttribute('src') === '') {
            //console.log('image ko => forcé');
            this.setAttribute('src', this.getAttribute('data-original'));
        }
    });
</script>
