  <script type="text/javascript">
      (function ($) {

          $(function () {
              $("#accordion").click(function (evt) {
                  evt.stopPropagation();
                  $('html, body').animate({scrollTop: $('#accordion').offset().top - 100}, 'slow');
                  //    return false;
              });

              if ($.trim($('div.tools.ddl-margin').html()).length == 0) {
                  $('div.tools.ddl-margin').css('width', '1px');

              } else {
                  $('div.content-content > div.left').css('padding-right', '150px');
              }

              $('.field-collection-container #small-tabs ul').each(function (index) {
                  if ($(this).html().trim().length == 0) {
                      $(this).parentsUntil('.field-collection-container').hide();
                  }
              });
            /* hide empty field division */
              var previousField = $('#accordion').parent().prev();
              if (previousField.html().length == 0) {
                  previousField.hide();
              }
          });
      })(jQuery);
  </script>
