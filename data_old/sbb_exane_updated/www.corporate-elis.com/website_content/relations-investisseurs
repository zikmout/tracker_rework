<script type="text/javascript">

jQuery(function($){
  $(document).ready(function() {
  $('#sub_menu a[href*=#]').removeClass("active");
/*var timestamp = '?timestamp=123';
$('img').each(function () {
  if ($(this).attr('src')) {
    $(this).attr('src', ($(this).attr('src') + timestamp));
  }
});*/
  $(".iframe").colorbox({iframe:true, width:"95%", height:"60%", maxWidth:"640px", maxHeight:"420px"});
  
  if ($('#zone-postscript-wrapper').length) {
  var headertop= 210;
var positionElementInPage = $('#zone-postscript-wrapper').offset().top;
};
/****************	*/
$(window).scroll(

    function() {
  if ($(window).scrollTop() >= positionElementInPage) {
		headertop = 39;
            // fixed
            $('#zone-postscript-wrapper').addClass("floatable");
        } else {
		headertop = 210;
            // relative
            $('#zone-postscript-wrapper').removeClass("floatable");
        }
    }
);

$("#sub_menu a, #button_perso").click(function(e){
	//var target = $(this).attr("rel");
		$('#sub_menu').find('a').each(function() {
    $(this).css("text-decoration","none");
});
	$(this).css("text-decoration","underline");
	var target = $(this).attr("href").split('#')[1];
	if(target)
	{
	var targetOffset = $('#'+target).offset().top;
	//$('html, body').animate({scrollTop: (targetOffset-200)+'px'}, 1000);
	$('html, body').animate({scrollTop: (targetOffset - headertop - 28)+'px'}, 600);
 e.preventDefault(); 
  }
});	

if ($("#messages").length > 0){
 var targetOffset = $('#messages').offset().top;
	//$('html, body').animate({scrollTop: (targetOffset-200)+'px'}, 1000);
	$('html, body').animate({scrollTop: (targetOffset - headertop - 28)+'px'}, 600);
}





  });
});

</script>



