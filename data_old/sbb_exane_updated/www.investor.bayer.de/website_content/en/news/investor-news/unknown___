        function classRemove(s, a) {
            s.classList ? s.classList.remove(a) : s.className = s.className.replace(new RegExp("\\b" + a + "\\b", "g"), "")
        }

        function classAdd(s, a) {
            s.classList ? s.classList.add(a) : classHas(s, a) || (s.className += " " + a)
        }

        function classHas(s, a) {
            return s.classList ? s.classList.contains(a) : new RegExp("\\b" + a + "\\b").test(s.className)
        }

        classRemove(document.documentElement, 'no-js');
        classAdd(document.documentElement, 'js');
