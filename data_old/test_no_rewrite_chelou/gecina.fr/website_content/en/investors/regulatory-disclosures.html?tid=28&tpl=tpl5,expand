          <section class="section">
                          <div class="region region-content">
    <div id="block-gecina-local-tasks" class="block block-core block-local-tasks-block">
  
    
                  </div>
<div id="block-gecina-content" class="block block-system block-system-main-block">
            
<article data-history-node-id="15810" role="article" about="/en/investors/regulatory-disclosures" typeof="schema:WebPage" class="node node--type-page node--view-mode-full">

    
          <span property="schema:name" content="Regulatory information" class="rdf-meta hidden"></span>


    
    <div class="node__content">
            

    <div class="field field--name-field-paragraphs field--type-entity-reference-revisions field--label-above field--page--internal">        
                <div class="field__items">
                                        <div class="field__item">
    

                        <section class="paragraph paragraph--type--gecina-banner paragraph--view-mode--default paragraph--type--banner paragraph--type--banner--internal relative">
        <div class="paragraph--type--banner__milestone absolute pin-b h-px mb-90 w-20 h-20"></div>
        <div class="paragraph--type--banner__wrapper relative bg-blue lg:fixed lg:pin-t lg:pin-l lg:w-full lg:z-0">
                                                                                <div class="paragraph--type--banner__background absolute w-full h-full pin-t pin-l bg-cover bg-center bg-no-repeat lg:hidden" style="background-image: url('/sites/default/files/styles/thumbnail_768x819/public/2019-04/MOB_HERO_375x400_informations_0.jpg?itok=afAJoA2S')"></div>
                                                                                                            <div class="paragraph--type--banner__background hidden absolute w-full h-full pin-t pin-l bg-cover bg-center bg-no-repeat lg:block" style="background-image: url('/sites/default/files/styles/large_1440x600/public/2019-04/DESK_HERO_1440x550_informations_0.jpg?itok=cP_NpxwX')"></div>
                                                    <div class="absolute w-full h-full pin-t pin-l bg-blue opacity-50 z-10"></div>
                        <div class="absolute pin-t pin-l w-full h-full z-20">
                <div class="container h-full">
                    <div class="h-full lg:-mx-12">
                        <div class="text-center flex content-center flex-col h-full px-20 justify-center lg:ml-1/6 lg:w-2/3 lg:px-12">
                                                            <h1 class="paragraph--type--banner__heading h2 h2--text-white mb-15">Regulated information</h1>
                                                                                        <div class="paragraph--type--banner__highlight">See our Group’s regulated information filed with the French financial markets regulator (AMF).</div>
                            
                                                    </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
</div>
                            <div class="field__item">







            <section class="paragraph paragraph--type--gecina-filter-documentation paragraph--view-mode--default paragraph--type--documents-with-filter relative bg-white gecina-component">

        <div class="container">
            <div class="flex flex-wrap lg:-mx-12">
                <div class="w-full lg:w-1/4 lg:px-12">

                                            <div class="paragraph--type--documents-with-filter__categories border-b border-solid lg:border-none lg:mb-30">
                            <label class="flex flex-row-reverse outline-none font-mont text-blue text-16 font-semibold leading-tight uppercase p-20 lg:font-bold lg:text-sand lg:p-0 lg:mb-10 lg:text-12 lg:block" for="mail"><span class="block flex-1 pr-20">CATEGORY</span></label>
                            <ul class="bg-beige py-20 list-reset font-mont leading-normal text-blue">
