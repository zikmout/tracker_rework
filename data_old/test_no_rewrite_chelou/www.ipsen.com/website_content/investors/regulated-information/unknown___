        <script>
          var leavingModal = new VanillaModal.default({ clickOutside: false, })
          var leavingWhiteList = "www.ipsenus.com/ www.ipsen.com.br/ www.ipsen.pl/ ipsen-pharma.de/ www.ipsen.com www.ipsen.co.uk/ www.ipsen.nl/ www.ipsen.ru/ www.ipsen.com.au/ www.ipsen-france.fr/ www.ipsen.es/  www.ipsenus.com/ ".split(' ')

          function closeLeavingModal() {
            leavingModal.close()
            return false
          }

          $( document ).ready(function() {

            if ($('.js-player').length) {
             const players = Plyr.setup('.js-player');
            }

            console.log('en-001')

            console.log('https://www.ipsen.com')
            $('a:not([href*="https://www.ipsen.com/"])').each(function() {
                $(this).not('.no-modal-override')
                .not('[href*="mailto:"]')
                .not('[href$=".pdf"]')
                .not('.mlp-language-nav-item a')
                .not('[href$="?nopop"]')
                .not('[href$=".ics"]')
                .not('[href^="#"]')
                .not('[data-popupcontent]').click(function() {
                var targetUrl = $(this).attr('href')
                var targetTarget = $(this).attr('target')
                var isExternal = true
                for (var n=0;n<leavingWhiteList.length;n++) {
                  if (targetUrl.indexOf(leavingWhiteList[n]) >= 0 && leavingWhiteList[n] != "") {
                    isExternal = false
                  }
                } 
                if (!isExternal) {
                  $('#modal-leaving .external-modal-content').addClass('hidden')
                  $('#modal-leaving .internal-modal-content').removeClass('hidden')
                } else {
                  $('#modal-leaving .external-modal-content').removeClass('hidden')
                  $('#modal-leaving .internal-modal-content').addClass('hidden')
                }
                leavingModal.open('#modal-leaving')
                $('#modal-leaving-continue').attr('href', targetUrl).attr('target', targetTarget)
                setTimeout(function(){
                  $('.modal'). mousedown(function(e){
                    e.preventDefault();
                    e.stopPropagation();
                  }); 
                },500);
                return false
              })
            })
            $('[href$=".pdf"]').attr('target','_blank')
          });

        $('#modal-leaving-continue').on( "click", function(e) {
          leavingModal.close()
        });

        $('[href$="?nopop"]').on( "click", function(e) {
            e.preventDefault()
            var url = $(this).attr('href')
            url = url.replace('?nopop', '')
            var target = $(this).attr('target')
            if (target == '_blank') {
              window.open(url,'_blank');
            } else {
              window.location.href = url    
            }         
        });

        $('[href*="doccheck.com"]').on( "click", function(e) {
            e.preventDefault()
            var vanmodal = new VanillaModal.default()
            var url = $(this).attr('href')
            var modal = $(this).next().attr('id')
            var modalID = '#' + modal
            vanmodal.open(modalID)
        });

        </script>

