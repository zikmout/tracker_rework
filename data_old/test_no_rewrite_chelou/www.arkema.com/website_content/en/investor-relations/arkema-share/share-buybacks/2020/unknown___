	// fonction de chargements asynchrones
	var select2Initialized_sendToFriends = undefined;
	var select2Initialized_defaults = [];
	function loadjsfile(filename,options,cb){
	    var fileref = document.createElement('script');
	        fileref.setAttribute("type","text/javascript");
	        fileref.setAttribute("src", filename);
	    if(options!==undefined && options.async)    
	        fileref.async = true;
	    if(options!==undefined && options.defer)    
	          fileref.defer = true;
      if(cb!==undefined){
          if(DEBUG) console.log('-- javascript.jsp -- loadjsfile '+ filename +' with callback',cb);
          fileref.setAttribute("onload", cb);
          /*fileref.onload = fileref.onreadystatechange = function() {
            if (fileref.readyState ||fileref.readyState === "loaded" || fileref.readyState === "complete"){
              // So all dependencies are ready, call the desired function now
              if(DEBUG) console.log('-- javascript.jsp -- load the callback...');
              cb();
            }
          }*/
      }else{
          if(DEBUG) console.log('-- javascript.jsp -- loadjsfile '+ filename +' without callback');
      }
	    if (typeof fileref!="undefined")
	      document.getElementsByTagName("head")[0].appendChild(fileref);
  }
	
	
	// fonctions d'activation du select2 dans la popin send-to-friends
	function select2matchEmail (term) {
	  var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	  if (re.test(term)) {
	    if(DEBUG) console.debug(term+' is a valid email');
	    return true;
	  }
	  if(DEBUG) console.warn(term+' is not an valid email');
	  return false;
	}
	function activateSelect2(){
	  try{
	    if(select2Initialized_sendToFriends!==undefined){
	      if(DEBUG) console.debug('-- javascript.jsp -- activateSelect2() : destroy current select2...');
	      select2Initialized_sendToFriends.select2("destroy");
	    }
	    if(DEBUG) console.debug('-- javascript.jsp -- activateSelect2() : initialize select2...');
	    select2Initialized_sendToFriends = $(".use-select2").select2({
	      width: '100%',
	      tags: true,
	      allowClear: true,
	      tokenSeparators: [',', ' ',';'],
	      placeholder: '',
	      selectOnClose: true, // permet de selectionner le tag courant sur un tab ou enter
	      createTag: function (params) {
	         if(DEBUG) console.debug(params);
	         if(params!==undefined){
	           var term = $.trim(params.term);
	           if (term === '') {
	             //console.warn(term+' is empty');
	             return null;
	           }
	           if (!select2matchEmail(term)) {
	             //console.warn(term+' is invalid');
	             return null;
	           }
	           if(DEBUG) console.debug(term+' is ok');
	           return {
	             id: term,
	             text: term,
	             newTag: true // add additional parameters
	           }
	         }else{
	           if(DEBUG) console.warn('-- javascript.jsp -- activateSelect2() : no params');
	           return null;
	         }
	       }
	    });
	  }catch(err){
	    console.error("-- javascript.jsp -- Error in activateSelect2() : ",err);
	  }
	}
	var callbackSelect2 = function(){
	  try{
		  setTimeout(function(){
			  //if (typeof select2 === 'function') {
	        if(DEBUG) console.debug('-- javascript.jsp -- callbackSelect2() : cool! select2 is a function');
	        var select2Initialized_sendToFriends = undefined;
	        var basicwindowwidth = $(window).width();
	        $(window).on("debouncedresize", function( event ) {
	         if($(window).width() != basicwindowwidth){
	           activateSelect2();
	           basicwindowwidth = $(window).width();
	         }
	        });
	        activateSelect2();
	      /*}else{
	        console.error('-- javascript.jsp -- Error in callbackSelect2(), select2 is not a function');
	      }*/
	        /*if($(".use-select2-default").length > 0){
            if(DEBUG) console.log('-- javascript.jsp -- .use-select2-default detected, load select2.multi-checkboxes.js...');
            loadjsfile(select2Activation2,{async:true});
          }*/
		  },100);
	  }catch(err){
	    console.error('-- javascript.jsp -- Error in callbackSelect2()',err);
	  }
	}
	
	// fonctions d'activation des highcharts
	function callbackHighchartsActivation(){
		try{
			if(highchartsConfigs.length>0){
	      for(var i = 0; i < highchartsConfigs.length; i++){
	        try{
	          var chart = new Highcharts.Chart(highchartsConfigs[i]);
	          if(DEBUG) console.debug('-- javascript.jsp -- callbackHighchartsActivation() : chart activated');
	        }catch(err){
	          console.error(e);
	        }
	      }
	    }
		}catch(err){
			console.error('-- javascript.jsp -- Error in callbackHighchartsActivation()',err);
		}
	}
	var callbackHighcharts1 = function(){
		loadjsfile(highchartsActivation2,{async:true},'callbackHighchartsActivation()');
	}
	
	// fonctions d'activation des flexsliders
	var callbackFlexslider = function(){
		try{
			if($(".slider").length>0){
        $(".slider").sliderBehavior();
        if(DEBUG) console.log('-- javascript.jsp -- callbackFlexsliders() : .slider activated.');
      }
      if($(".imagegallery-slider").length>0){
         $(".imagegallery-slider").each(function(){
           var $mainSlider = $(this);
           var $secondSlider = $mainSlider.next();
           var big = '#' + $(this).attr("id");
           var small = '#' + $(this).attr("id") + '-carousel';
           var info = '#' + $(this).attr("id") + '-info';
           var myInfo = $(this).next().next(".info");
           
           if(DEBUG) console.log('-- javascript.jsp -- callbackFlexsliders() .imagegallery-slider : ' + big + ' contains : ' + ($(small).length>0 ? small + ' exists ' : small + 'doesnt exists') + ($(big).length>0 ? big + ' exists ' : big + 'doesnt exists'));
           
           // modification de la width au resize
           var basicwindowwidth = $(window).width();
           $(window).on("debouncedresize", function( event ) {
             if($(window).width() != basicwindowwidth){
               $mainSlider.hide();
               $secondSlider.hide();
               $mainSlider.width($mainSlider.parent().innerWidth());
               $mainSlider.show();
               $secondSlider.show();
               basicwindowwidth = $(window).width();
             }
           });
           // activation
           $(small).flexslider({
             animation: "slide",
             controlNav: false,
             slideshow: false,
             itemWidth: 138,
             itemMargin: 2,
             asNavFor: big
           });
           $(big).flexslider({
             animation: "slide",
             controlNav: false,
             animationLoop: false,
             slideshow: false,
             sync: small,
             init: function(slider){
               myInfo.html($(slider.slides[slider.currentSlide]).children(".infos").html());
             },
             after: function(slider){
               myInfo.html($(slider.slides[slider.currentSlide]).children(".infos").html());
             }
           });
           if(DEBUG) console.log('-- javascript.jsp -- callbackFlexsliders() .imagegallery-slider : ' + big + ' activated.');
           
           // refresh du equalheight des paragraphs
           if ($(".paragraph--with2cols,.paragraph--with3cols").length>1) {
           if(DEBUG) console.log('-- javascript.jsp -- callbackFlexsliders() .imagegallery-slider : responsiveEqualHeights run on ".paragraph--with2cols,.paragraph--with3cols');
           $(".paragraph--with2cols,.paragraph--with3cols").responsiveEqualHeights({
             child: '>.paragraph-column',
             formats : ['desktop','tablet-stretched','tablet-flexible'],
             allowInLandingPage : true
           });
         }
        });
      }
    }catch(err){
      console.error('-- javascript.jsp -- Error in callbackFlexsliders()',err);
    }
  }
	
  $(function() {
		// Activation du plugin pour le tableMode
		if($('#productsTable').length > 0){
			if(DEBUG) console.log('-- javascript.jsp -- #productsTable detected, load dataTableMode.js...');
      loadjsfile(dataTableActivation,{async:true});
    }
		if($('#share-price-container, #share-price-teaser-container').length > 0){
	    if(DEBUG) console.log('-- javascript.jsp -- Want to display share-price information, loading js...');
			loadjsfile(sharepriceJS,{async:true});
		}
		if($('.share-information').length > 0){
			loadjsfile(stockchartJS,{async:true});
		}
		if($('.share-information-fis').length > 0){
		  loadjsfile(stockchartJSFIS,{async:true});
		}
		if($('#centercolumn .search-results, .news-list-large').length > 0){
			loadjsfile(searchJS,{async:true});
		}
		if($('.pname input').length > 0 || $("#header-search-form #searchbar").length>0){
			loadjsfile(searchProductNameJS,{async:true});
		}
		if($('.range-product, .range-product-finder, .contact-availability').length > 0){
			loadjsfile(rangeproductJS,{async:true});
		}
		if($('.contact-form, .changepassword-form, .login-form, .newsalert-form, .newsletter-form, .profile-form, .register-form').length > 0){
			loadjsfile(contactformJS,{async:true});
		}
		// Ajout du plugin select2 pour l'entree d'emails dans la popin d'envoi du panier a des amis ET pour les select multiple du product-selection-guide
		if($(".use-select2").length > 0 || $(".use-select2-default").length > 0){
			if(DEBUG) console.log('-- javascript.jsp -- .use-select2 detected, load select2-4.0.3.full.js...');
			loadjsfile(select2Activation,{async:true},'callbackSelect2()');
	  }
	if($('.productselectionguide-pim').length > 0){
	    if(DEBUG) console.log('-- javascript.jsp -- .productselectionguide PIM detected, load product-selection-guide-manager-pim.js...');
	    loadjsfile(productSelectionGuidePimActivation,{async:true});
	}
    if($('.productselectionguide').length > 0){
      if(DEBUG) console.log('-- javascript.jsp -- .productselectionguide detected, load product-selection-guide-manager.js...');
      loadjsfile(productSelectionGuideActivation,{async:true});
    }
		// Chargement des highcharts
		if(needHighcharts){ // initialisee dans le main.jsp, et passee a true dans les chart.jsp
      if(DEBUG) console.log('-- javascript.jsp -- .use-select2 detected, load select2-4.0.3.full.js...');
      loadjsfile(highchartsActivation1,{async:true},'callbackHighcharts1()');
    }
		// Chargement des flexsliders
    if((window.flexslider == undefined) && $(".slider,.imagegallery-slider").length > 0){ 
      if(DEBUG) console.log('-- javascript.jsp -- .slider,.imagegallery-slider detected, load flexslider js...');
      loadjsfile(flexsliderActivation,{async:true},'callbackFlexslider()');
    }
    // Activation des reCaptcha
    if($(".g-recaptcha").length > 0){ 
      if(DEBUG) console.log('-- javascript.jsp -- .g-recaptcha detected, load reCaptcha js...');
      loadjsfile(reCaptchaActivation,{async:true,defer:true});
    }
    // REACT - product-comparator
    if($('#react--product-comparator').length > 0){
      if(DEBUG) console.log('-- javascript.jsp -- #react--product-comparator detected, load polyfill...');
      loadjsfile('https://cdn.polyfill.io/v2/polyfill.min.js',{async:true});
      if(DEBUG) console.log('-- javascript.jsp -- #react--product-comparator detected, load react files...');
      loadjsfile(reactPCActivation,{async:true});
      if(DEBUG) console.log('-- javascript.jsp -- #react--product-comparator detected, load dedicated table2CSV...');
      loadjsfile(reactPCTable2CSV,{async:true});
      if(reactPCcssIncluded === undefined || !reactPCcssIncluded)   // reactPCcssIncluded defini dans css.jsp
        $("head").append('<link rel="stylesheet" href="'+reactPCcss+'" />');
    }
  });
</script>
