            var waypoint = new Waypoint({
              element: document.getElementById('page-main'),
              handler: function(direction) {
                $('.bourse-widget').toggleClass('bourse-widget--white')
              },
              offset: function() {
                return $('.masthead').outerHeight()
            }
            });
