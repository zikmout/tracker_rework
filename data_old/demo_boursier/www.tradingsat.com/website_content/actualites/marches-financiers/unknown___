    function refreshIframe()
    {
        var event = new Date();
        var hours = event.getHours();
        var minutes = event.getMinutes();
        var weekday = event.getDay();
        $(".graph-refresh").each(function(i, obj) {
            var urlInit = $(this).attr("src");
            if (urlInit.indexOf("TSStaticHTML5_1JCAC") != -1
                || urlInit.indexOf("TSStaticHTML5_1J") != -1) {
                if ((
                    weekday != 0
                    && weekday != 6
                    && hours >= 9
                    && (hours < 17 || (hours == 17 && minutes < 40))
                )) {
                    if ((urlInit.indexOf("TSStaticHTML5_1JCAC") != -1) && (urlInit.indexOf("it-finance.com/TSStaticHTML5_1JCAC") == -1)) {
                        var d = new Date();
                        var cleanUrl = $(this).attr("src").substr(0, $(this).attr("src").indexOf("TSStaticHTML5_1JCAC_"));
                        var url = (cleanUrl != "") ? cleanUrl : $(this).attr("src");
                        var minute = d.getMinutes();
                        if (minute >= 5) {
                            minute = minute-5;
                        } else {
                            minute = 60 + minute - 5;
                        }
                        $(this).attr("src", url + "TSStaticHTML5_1JCAC_" + d.getFullYear() + ("0" + (d.getMonth() + 1)).slice(-2) + ("0" + d.getDate()).slice(-2) + ("0" + d.getHours()).slice(-2) + ("0" + (minute)).slice(-2) + ".png");
                    } else {
                        var cleanUrl = $(this).attr("src").substr(0, $(this).attr("src").indexOf("time") - 1);
                        var url = (cleanUrl != "") ? cleanUrl : $(this).attr("src");
                        $(this).attr("src", url + "&time=" + new Date().getTime());
                    }
                }
            } else if (urlInit.indexOf("TSStaticHTML5_1JForex") != -1) {
                if (weekday != 6
                    && ((weekday == 0 && hours >= 23)
                        || (weekday == 5 && hours < 22)
                        || (weekday > 0 && weekday < 5)
                    )
                ) {
                    var cleanUrl = $(this).attr("src").substr(0, $(this).attr("src").indexOf("time") - 1);
                    var url = (cleanUrl != "") ? cleanUrl : $(this).attr("src");
                    $(this).attr("src", url + "&time=" + new Date().getTime());
                }
            } else {
                var cleanUrl = $(this).attr("src").substr(0, $(this).attr("src").indexOf("time") - 1);
                var url = (cleanUrl != "") ? cleanUrl : $(this).attr("src");
                $(this).attr("src", url + "&time=" + new Date().getTime());
            }
        });
    }
    </script>

