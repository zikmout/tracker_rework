						</div>
				<script>
			$( document ).ready(function() {
				console.log("ready");
				$( "#time-from" ).datepicker();
				$( "#time-to" ).datepicker();
				$( "#time-from" ).datepicker( "option", $.datepicker.regional[ 'en' ] );
				$( "#time-to" ).datepicker( "option", $.datepicker.regional[ 'en' ] );
				$( "#time-from" ).datepicker( "option", "dateFormat", "mm-dd" );
				$( "#time-to" ).datepicker( "option", "dateFormat", "mm-dd" );
				console.log("end");
			  } );
			  
			function loadMore() {

				/*$.post("/system/modules/it.hfarm.italgas/elements/newsList.jsp", */
				$.post("/en/newsList.jsp",
					   $("form#fmid").serializeArray(),
					function(txt) {
						$("#loadMoreButtonId").remove();
						$("div#TB_ajaxContent").append(txt);
						var loadedRes = Number($( "#loadedResourcesId" ).val()) + Number($( "#pageSizeId" ).val());
						$( "#loadedResourcesId" ).val(loadedRes);
						/*var pageIdx = Number($( "#pageIndexId" ).val()) + 1;
						$( "#pageIndexId" ).val(pageIdx);*/

					}
				);
			}
			
			function loadMoreFirst(numFound) {
				$('#resultFoundId').val(numFound);
				/*$.post("/system/modules/it.hfarm.italgas/elements/newsList.jsp", */
				$.post("/en/newsList.jsp",
					   $("form#fmid").serializeArray(),
					function(txt) {
						$("#loadMoreButtonId").remove();
						$("div#TB_ajaxContent").append(txt);
						var loadedRes = Number($( "#loadedResourcesId" ).val()) + Number($( "#pageSizeId" ).val());
						$( "#loadedResourcesId" ).val(loadedRes);
						/*var pageIdx = Number($( "#pageIndexId" ).val()) + 1;
						$( "#pageIndexId" ).val(pageIdx);*/

					}
				);
			}
			
			function resetForm(){
				$("#time-year").val('');
				$("#time-from").val('');
				$("#time-to").val('');
				$( "input:checked" ).prop('checked', false);
				var URL = window.location.href;
				index = URL.indexOf('?');
				if(index != -1){
					URL = URL.substring(0, index);
				}
				$('#fmid').attr('action',URL).submit();
				//$("#fmid").submit();
			}
		</script>
		
		
	</div>
		

		
		
	

