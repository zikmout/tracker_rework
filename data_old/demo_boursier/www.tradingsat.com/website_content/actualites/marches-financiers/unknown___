<script type="text/javascript">
    // Use in bandeau.html.twig
    function filterPalmaresIndice(data) {
        var marp = data.attr('data-uid');
        var send = {"marp": marp, 'varp': 'VariationPercent'};
        var url_glob = '//www.tradingsat.com/';
        $.ajax({
            type: 'GET',
            url: url_glob +'async/html/palmares-bourse-filter.php',
            data: send
        }).done(function(response) {
            var obj = JSON.parse(response);
            $('.bloc .gainers').html('').append(obj.gaimers);
            $('.bloc .loosers').html('').append(obj.loosers);
        }).fail(function(e) {
            console.log(e)
        })
    }
</script>


