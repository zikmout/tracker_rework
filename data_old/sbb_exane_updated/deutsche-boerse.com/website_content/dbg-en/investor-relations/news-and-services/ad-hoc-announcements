		$(document).ready(function() {
			$("#input-9e1fc06c-86e0-402e-a469-8180816bbbb8 input").autocomplete({
				source : function(request, response) {
					$.ajax({
						url : '/dbg-en/investor-relations/news-and-services/ad-hoc-announcements/588!suggest',
						type: 'POST',
						dataType : 'json',
						data : $("#input-9e1fc06c-86e0-402e-a469-8180816bbbb8 input").closest("form").serialize(),
