	$( window ).resize(function() {
		
		var isMobile = window.matchMedia("only screen and (max-width: 978px)").matches;
		
		if(!isMobile) {
		
	  		if($(".nav-collapse4").hasClass('in')){
	  			$("#mobile-search").addClass("collapsed");
				$( ".nav-collapse4" ).collapse('hide');
			}
			$("#search-white").show();
	  		$("#search-black").hide();
								
			if($(".nav-collapse5").hasClass('in')){
				$("#mobile-language").addClass("collapsed");
				$( ".nav-collapse5" ).collapse('hide');
			}
			$("#flag-white").show();
	  		$("#flag-black").hide();
	  		
	  		if($(".nav-collapse3").hasClass('in')){
	  			$("#mobile-menu").addClass("collapsed");
	  			$( ".nav-collapse3" ).collapse('hide');
	  		}
	  		
	  		$(".dropdown").removeClass("open");
		}
	});
