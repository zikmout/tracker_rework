		<script src="/scripts/js.cookies.js"></script>	
	<style>	
		.flyUp{display:none;left:40%;width:275px;height:120px;background-color:transparent;position:fixed;padding:0;text-align:right;bottom:0;z-index:10}.flyUpContents,.flyUpForMobile{background-color:#fff;bottom:0}.flyUpContents{border:thin solid gray;float:left;box-shadow:5px 5px 5px grey;border-radius:5px 5px 0 0;height:100px;position:absolute;width:260px}.flyUpForMobile{display:none;width:100%;height:115px;border:thin solid gray;left:0;position:fixed;padding-left:35px}.flyUpForMobileCloser{float:right;font-size:x-large;font-weight:700;padding-right:35px}.flyUpForMobileCloser a{text-decoration:none;color:#000}
	</style>
	
			
	<script>
			$(document).ready(function(){
				
				
				//this method reports window size  onLoad as well which is handy	
				$( window ).resize(function() {
  					//alert($(window).width());
					//append flyout or flyin div wrapped in an IF statement
						if($(window).width() > 800){
							if(!Cookies.get('CAREER_FLYIN')){//show flyIn	
								//alert('Greater than 800');
								$(".flyUpForMobile").fadeOut();
								$(".flyUpForMobileCloser").fadeOut();
								$(".flyUp").fadeIn();
								$(".flyUpCloser").fadeIn();
							}							   
						}else{
							//alert('Less than 800');
							if(!Cookies.get('CAREER_FLYIN')){//show flyIn	
								$(".flyUp").fadeOut();
								$(".flyUpCloser").fadeOut();
								$(".flyUpForMobile").fadeIn();
								$(".flyUpForMobileCloser").fadeIn();
							 }
						};
				});
				
				
				
				
				
				//flyUp closer is clicked
				$(".flyUpCloser a").on("click",function(e){
					e.preventDefault();
					$(".flyUp").fadeOut();
					Cookies.set('CAREER_FLYIN', 'TRUE', { expires: 365 });
				});
				
				
				
				//flyUpForMobile closer is clicked
				$(".flyUpForMobile a").on("click",function(e){
					//e.preventDefault();
					$(".flyUpForMobile").fadeOut();
					Cookies.set('CAREER_FLYIN', 'TRUE', { expires: 365 });
				});
			});		
	</script>	
</body>


	<script src="/scripts/common.min.js"></script>	
