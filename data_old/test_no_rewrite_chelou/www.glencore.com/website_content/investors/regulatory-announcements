				<script>
					$( document ).ready(function() {
						checkWidths();
						var resizeId;
						$( window ).resize(function() {
							clearTimeout(resizeId);
					    	resizeId = setTimeout(checkWidths, 500);
						});
					});

					function checkWidths() {
						if (window.matchMedia('(max-width: 575px)').matches) {
							//disregard left and right paddings
							var h = $(window).height();
							if (h < 1050) h = 1050;
							var mleft = $('#timeline').parents('.container').length + $('#timeline').parents('.container-fluid').length;
							mleft = mleft  15  -1;
							console.log ('containers:' + mleft);
							$('#timeline').css({'width': $('html').width(), 'height': h, 'margin-left': mleft});
						}
						else {
							$('#timeline').removeAttr('style');
						}
					}
				</script>
				
				<style>
					iframe {
						border: none;
						overflow: hidden;
						width: 100%;
						height: 1050pxpx;
					}
				</style>
		
