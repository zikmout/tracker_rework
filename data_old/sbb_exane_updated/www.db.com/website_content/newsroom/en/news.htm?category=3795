<script type="text/javascript">
var pageconfig = {
    contentId : "/newsroom/en/news.htm"
    
};
var wt = new webtrekkV3(pageconfig);

/* extra code for double account and invisible tags tracking */
var basicContentId = pageconfig.contentId;

var pageconfig2 = $.extend({
    trackId : '286813086616891'
    , trackDomain : 'tp.deutsche-bank.de'
    , cookie : '1'
    , fileSuffix : true
}, pageconfig)
var wt2 = new webtrekkV3(pageconfig2);

if (typeof tagsInvisibleTracking != 'undefined') {
  var tagsInvisibleTracking = ";" + tagsInvisible.replace(/,/g, ";") + ";";
  var tagsInvisibleTrackingCommaSeparated = "," + tagsInvisible.replace(/,/g, ",") + ",";
  wt.customParameter = {
    2: tagsInvisibleTracking,
    3: tagsInvisibleTrackingCommaSeparated,
  };
  wt2.customParameter = {
    2: tagsInvisibleTracking,
    3: tagsInvisibleTrackingCommaSeparated,
  };
}

if(typeof(wt2.sendinfo) != 'undefined') wt2.sendinfo();
//send UrlUpdate tracking for both accounts
$(window).bind('filterchange', function(ev,evData){
     pageconfig.contentId = basicContentId + evData.search;
     if(typeof(wt.sendinfo) != "undefined") wt.sendinfo(pageconfig);
     
     pageconfig2.contentId = basicContentId + evData.search;
     if(typeof(wt2.sendinfo) != 'undefined') wt2.sendinfo(pageconfig2);
});
/**/
 
if(typeof(wt.sendinfo) != "undefined") wt.sendinfo();
</script>

