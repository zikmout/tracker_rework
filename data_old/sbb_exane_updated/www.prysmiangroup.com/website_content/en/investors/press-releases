    jQuery(".prys2-search-form").submit(function() {  
       
        var keywords = jQuery(this).find(".search-input-text").val();
        var keywords1 ='';
        var keywords2 ='';
        keywords1 = keywords.replace(/\s+/g,' OR '); 
        keywords2 = keywords.replace(/\s+/g,'+');
        jQuery(this).attr("action", "/en/search/node/" + keywords1 + " language:en-us?query="+keywords2);        
    });        
