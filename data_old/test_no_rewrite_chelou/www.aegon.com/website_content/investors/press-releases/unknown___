    <div class="lightbox__content choose-cookies">
        <h4 class="choose-cookies__heading">Which information are you happy to share?</h4>

        <div class="choose-cookies__text">
            <p>Aegon.com uses two types of cookies. You can select the types of information you're willing to share below.</p>
                <p><a href="/link/e068a742e1df4906b15ae1f619b6c0a5.aspx" tabindex="2">Learn more about the cookies we use...</a></p>

            </div>
        
        <!-- cookie levels -->
        <form action="" class="choose-cookies__types">

            <div class="choose-cookies__levels"                
                :data-cookie-id="cookieConfig.hash">

                <div class="choose-cookies-level"
                    v-for="level in cookieConfig.levels">
                    <div class="choose-cookies-level__image">
                        <img class="choose-cookies-level__img" 
                            :alt="level.name"
                            :src="(level.selected) ? level.activeimage : level.nonactiveimage"
                            :data-img-on="level.activeimage" 
                            :data-img-off="level.nonactiveimage">
                    </div>
                    <div class="choose-cookies-level__header">
                        <h5>
                            <label class="choose-cookies-level__checkbox control control--checkbox">
                                {{ level.name }}
                                <input :id="'cookieLevel' + level.priority" 
                                    type="checkbox" 
                                    name="cookie" 
                                    :value="level.priority" 
                                    :checked="level.selected" 
                                    :disabled="level.priority == 1 ? true : false"
                                    v-on:change="setLevel">
                                    <div class="control__indicator"
                                    tabindex="2"></div>
                            </label>
                        </h5>
                    </div>
                    <div class="choose-cookies-level__description">
                        {{ level.description }}
                    </div>
                </div>

            </div>

        </form>
        <!-- /cookie levels -->

        <div class="lightbox__actions">
            <button class="button button--primary js-close"
                    v-cookie-message="{type:''}" 
                    type="button" 
                    :data-accept-cookies-max-level="cookieMaxLevel"
