			<script type="text/javascript">
	function changeLanguage(language) {
	    AUI().ready('liferay-portlet-url', function(A) {
	        var renderURL = Liferay.PortletURL.createActionURL();
	        	        var redirectURL = '\x2fen\x2finvestor-relations\x2ffinancial-press-releases';
	        
	        renderURL.setPortletId("82");
	        renderURL.setWindowState("normal");
	        renderURL.setPortletMode("view");
	        
			renderURL.setParameter("struts_action", "/language/view")
			renderURL.setParameter("redirect", redirectURL)
			renderURL.setParameter("languageId", language)
	        
	        window.location = renderURL.toString();
	    });
	}
</script>	
