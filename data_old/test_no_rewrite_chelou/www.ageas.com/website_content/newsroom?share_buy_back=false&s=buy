<script type="text/javascript">

        // DATALAYER GOOGLE TAG MANAGER
        // ----------------------------

        var language = 'en';
        var contactName = 'newsroom';

        if(typeof view_pageName != 'undefined')
            window.pageName = view_pageName;

        else
            window.pageName = 'AGEAS:newsroom';

        function getResponsive(){
            var width = window.innerWidth;
            var breakpoint;
            if(width >= 0 && width <= 767){
                breakpoint = 'mobile';
            }else if(width >= 768 && width <= 1024){
                breakpoint = 'tablet';
            }else {
                breakpoint = 'desktop';
            }
            return breakpoint;
        }

        if (dataLayer) {
          dataLayer.push({
            'pageName' : window.pageName,
            'language' : language,
            'template_responsive': getResponsive(),
            'section' : 'newsroom'
          });
        }

    </script>

