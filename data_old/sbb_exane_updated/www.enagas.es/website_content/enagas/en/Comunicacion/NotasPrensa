<script src="/WEBCORP-static/maqueta/js/libs/jquery-1.9.1.min.js"></script>

<script type="text/javascript">

	//Al cargar la pagina comprueba si tiene que buscar contra GSA y lanza la busqueda
	$(document).ready(function(){
		var buscarGSA = false;
		if(buscarGSA)
		{
			//Lanzamos el spinner
			$('#overlay-spinner').show();
			
			//Recuperamos los parametros de la busqueda
			var language = 'es';
			var keywords = '';
			var fecha_inicio = '';
			var fecha_fin = '';
			var start = '';
			var num = '5';
			
			//Preparamos la url para realizar la consulta
			var url_string = "/web-corporativa-ext-templating/webcorp/buscadorNotasPrensa/buscarNotasPrensaGSA?fecha_inicio=".concat(fecha_inicio)
					.concat("&fecha_fin=").concat(fecha_fin)
					.concat("&keywords=").concat(keywords)
					.concat("&language=").concat(language)
					.concat("&start=").concat(start)
					.concat("&num=").concat(num);
			
			var contentTypeVersion = "";
			
			var ua = window.navigator.userAgent;
		    var msie = ua.indexOf("MSIE ");

		    if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))
		    {
		    	contentTypeVersion = "application/x-www-form-urlencoded;charset=ISO-8859-15";
		    }
		    else  
		    {
		    	contentTypeVersion = "application/x-www-form-urlencoded;charset=UTF-8";
		    }
			
					
			//Hacemos la peticion y obtenemos los resultados
			$.ajax({
		      	url: url_string,
		      	type: 'POST',
		      	
		      	/* contentType: "application/x-www-form-urlencoded;charset=UTF-8",
		      	requestHeader: "'content-type', 'application/x-www-form-urlencoded;charset=UTF-8'", */
		      	contentType: contentTypeVersion,
		      	mimeType: 'text/html',
		      	success: function(data) 
		      	{
					//Ocultamos el spinner
					$('#overlay-spinner').hide();
					
					//Si hay respuesta la procesamos
					if(data)
					{
						//Insertamos el html de respuesta en la capa de resultados
						$('#resutadoGSA').html(data);
						
						//Si existe la capa listadoResultadosGSA dentro de resutadoGSA mostramos la capa de resultados
						var resultados = document.getElementById('listadoResultadosGSA');
						if(resultados)
						{
							$('#listadoResultados').html(resultados.innerHTML);
							$('#listadoResultados').show();
							
							//Si tambien hay paginacion mostramos la paginacion
							var paginacion = document.getElementById('paginacionGSA');
							if(paginacion)
							{
								$('#paginacionSuperior').html(paginacion.innerHTML);
								$('#paginacionInferior').html(paginacion.innerHTML);
								
								$('#paginacionSuperior').show();
								$('#paginacionInferior').show();
							}
							
							//Mostramos el enlace de descarga de adobe
							$('#descargaAdobe').show();
						}
						//Si no hay resultados comprobamos si ha habido algun error o es que no hay resultados
						else 
						{
							//Si existe la capa errorGSA dentro de resutadoGSA mostramos la capa de error
							var errorGSA = document.getElementById('errorGSA');
							if(errorGSA)
							{
								$('#f_mens_error').show();
							}
							//Si no hay error comprobamos si existe la capa noHayDatosGSA para mostrar el mensaje de que no hay resultados
							else
							{
								var noHayDatosGSA = document.getElementById('noHayDatosGSA');
								if(noHayDatosGSA)
								{
									//Abrimos la ventana modal informando de que no hay resultados
									simpleModal.openModal(literal["noHayDatos"]);
									document.location.href='https://www.enagas.es/enagas/es/Comunicacion/NotasPrensa';
								}
							}
						}
					}
					//Si no hay respuesta informamos de que ha habido un error al realizar la busqueda
					else
					{
						$('#f_mens_error').show();
					}
					
					//Una vez procesado todo eliminamos la capa oculta con todo el html devuelto por la consulta
					$('#resutadoGSA').remove();
					
		  		},
		  		error: function (request, status, error) 
		  		{
		  			//Ocultamos el spinner
					$('#overlay-spinner').hide();
					
		  			//Informamos del error
		  			$('#f_mens_error').show();
		  			
		  			//Eliminamos la capa oculta con todo el html devuelto por la consulta
					$('#resutadoGSA').remove();
		  		}
			});
		}
		//Si cargamos los contenidos de VCM mostramos siempre el enlace de descarga de adobe
		else
		{
			//Mostramos el enlace de descarga de adobe
			$('#descargaAdobe').show();
		}
	});
	
	//Hace la validacion del formulario y si todo es correcto hace el submit, si no muestra el mensaje de error correspondiente
	function validarFormulario()
	{
		//LITERALES CON LOS MENSAJES DE ERROR EN MULTIIDIOMA PARA EL OPENMODAL
		var mensajesDeError = {
			fechaNoCorrecta: literal["fecha_incorrecta"],
			fechaInicio: literal["fecha_inicio"],
			fechaFin: literal["fecha_fin"],
			fechaMayorQue: literal["mayor_que"],
			camposVacios: literal["camposVacios"]
		}
		
		//Los campos a validar
		var keywords = $('#keywords').val();
		var sDateFrom = $('#fecha_inicio').val();
	    var sDateTo = $('#fecha_fin').val();
	    
	    //Variable que indica si hay algun error de validacion
	    var errorValidacion = false;
		
		//Comprobamos si todos los campos estan vacios
		if(keywords.length===0 && sDateFrom.length===0 && sDateTo.length===0)
		{
			errorValidacion = true;
			simpleModal.openModal(mensajesDeError.camposVacios);
		}
		//Si se ha rellenado algun campo los validamos
		else
		{
			//Creamos los objetos Date a partir de las datos introducidos
			var language = 'es';
			var sDateFromToDate = sDateFrom;
			var sDateToToDate = sDateTo;
			
			if (language=="es")
			{
				sDateFromToDate = changeDate(sDateFrom);
				sDateToToDate = changeDate(sDateTo);
			}
			var sDateFromDate = new Date (sDateFromToDate);
			var sDateToDate = new Date (sDateToToDate);
			
			var anioFrom = sDateFromDate.getFullYear(); 
			var anioTo = sDateToDate.getFullYear();
			
			var esBisiesto =  (((anioFrom % 4 == 0) && ((anioFrom % 100 != 0) || (anioFrom % 400 == 0))) || ((anioTo % 4 == 0) && ((anioTo % 100 != 0) || (anioTo % 400 == 0))))
			
			//Validamos la fecha de inicio si esta rellena
			if (sDateFrom.length>0 && !validaFechaDDMMAAAA(sDateFrom))
			{
				errorValidacion = true;
				simpleModal.openModal(mensajesDeError.fechaNoCorrecta+" "+sDateFrom);
			}
			//Validamos la fecha de fin si esta rellena
			else if (sDateTo.length>0 && !validaFechaDDMMAAAA(sDateTo))
			{
				errorValidacion = true;
				simpleModal.openModal(mensajesDeError.fechaNoCorrecta+" "+sDateTo);
			}					
			//Si las dos fechas estan rellenas la fecha hasta debe ser superior a la fecha desde
			else if (sDateFrom.length>0 && sDateTo.length>0)
			{
				if(sDateFromDate>sDateToDate)
				{
					errorValidacion = true;
					simpleModal.openModal(mensajesDeError.fechaInicio+" "+mensajesDeError.fechaMayorQue+" "+mensajesDeError.fechaFin);
				}
			}
		}
		
		//Si no hay errores de validacion enviamos el formulario
		if(!errorValidacion)
		{
			document.getElementById('formularioBuscador').submit();
		}
	}
	
	//CAMBIA LA FECHA DE FORMATO DD/MM/YYYY A MM/DD/YYYY Y VICEVERSA
	function changeDate(sDate)
	{
		var newDate = sDate.split('/'); 			
		sDate = newDate[1] + "/" + newDate[0] + "/" + newDate[2];
		return sDate;
	}
	
	//FUNCION QUE VALIDA QUE LAS FECHAS EXISTAN EN UN CALENDAR
	function validaFechaDDMMAAAA(fecha)
	{
		var language = 'es';
		if (language=="en")
		{
			fecha = changeDate(fecha)
		}
		var dtCh= "/";
		var minYear=1900;
		var maxYear=2100;
		
		//VALIDA QUE NO SE META SOLO UN NUMERO ENTERO
		function isInteger(s)
		{
			var i;
			for (i = 0; i < s.length; i++)
			{
				var c = s.charAt(i);
				if (((c < "0") || (c > "9"))) return false;
			}
			return true;
		}
		//VALIDA QUE NO SEAN CARACTERES
		function stripCharsInBag(s, bag)
		{
			var i;
			var returnString = "";
			for (i = 0; i < s.length; i++)
			{
				var c = s.charAt(i);
				if (bag.indexOf(c) == -1) returnString += c;
			}
			return returnString;
		}
		//VALIDA LOS DIAS DE FEBRERO POR SI ES BISIESTO
		function daysInFebruary (year)
		{
			return (((year % 4 == 0) && ( (!(year % 100 == 0)) || (year % 400 == 0))) ? 29 : 28 );
		}
		function DaysArray(n) 
		{
			for (var i = 1; i <= n; i++) 
			{
				this[i] = 31
				if (i==4 || i==6 || i==9 || i==11) {this[i] = 30}
				if (i==2) {this[i] = 29}
			}
			return this
		}
		
		//VALIDA SI ES UNA FECHA VALIDA
		function isDate(dtStr)
		{
			var daysInMonth = DaysArray(12)
			var pos1=dtStr.indexOf(dtCh)
			var pos2=dtStr.indexOf(dtCh,pos1+1)
			var strDay=dtStr.substring(0,pos1)
			var strMonth=dtStr.substring(pos1+1,pos2)
			var strYear=dtStr.substring(pos2+1)
			strYr=strYear
			if (strDay.charAt(0)=="0" && strDay.length>1) strDay=strDay.substring(1)
			if (strMonth.charAt(0)=="0" && strMonth.length>1) strMonth=strMonth.substring(1)
			for (var i = 1; i <= 3; i++) 
			{
				if (strYr.charAt(0)=="0" && strYr.length>1) strYr=strYr.substring(1)
			}
			month=parseInt(strMonth)
			day=parseInt(strDay)
			year=parseInt(strYr)
			if (pos1==-1 || pos2==-1)
			{
				return false
			}
			if (strMonth.length<1 || month<1 || month>12)
			{
				return false
			}
			if (strDay.length<1 || day<1 || day>31 || (month==2 && day>daysInFebruary(year)) || day > daysInMonth[month])
			{
				return false
			}
			if (strYear.length != 4 || year==0 || year<minYear || year>maxYear)
			{
				return false
			}
			if (dtStr.indexOf(dtCh,pos2+1)!=-1 || isInteger(stripCharsInBag(dtStr, dtCh))==false)
			{
				return false
			}
			return true
		}
		
		//DEVUELVE TRUE O FALSE SI LA FECHA ES VALIDA O NO
		if(isDate(fecha))
		{
			return true;
		}
		else
		{
			return false;
		}
	}

</script>


  
  


