<!-- Libreria Webtrekk 4.4.1, (c) www.webtrekk.com -->
<script type="text/javascript" src="https://images.finecobank.com/common/pub/js/webtrekk_v4.min.js"></script>
<script type="text/javascript">
var currentPage = "Corporate";
function getContentIdByURL() {
try {
var url = (document.location.href || 'no_content').split('#')[0].split('?')[0].toLowerCase();
var split_anchors = (document.location.href || '').split('#');
var anchor_value = '';
if (split_anchors.length > 1)
anchor_value = '#' + split_anchors[1].toLowerCase();
return url + anchor_value;
}
catch (err) {
console.log('WTK - an error was encountered while cleaning up the URL, tracking document.location.href instead.\n' + err);
return document.location.href;
}
}
var pageConfig = {
linkTrack: "", // Rilevazione dei LINK ["link" o "standard" o "" per disattivato]
heatmap: "", // Attivare la rilevazione della HEATMAP ["1" = on | "0" = off]
form: "", // Attivare la rilevazione dei FORM ["1" = on | "0" = off]
contentId: getContentIdByURL() // Popola la variabile con la URL completa della pagina
};
var wt = new webtrekkV3(pageConfig);
wt.contentGroup = {
1: "Fineco", // Aggregato
2: "finecobank.com", // Sito
3: "it", // Country
4: "public", // Canale
5: currentPage // Sotto canale
};
wt.customParameter = {
1: document.title,
2: "it" // Country
};
wt.sendinfo();
</script>
