                        data-object-fit="cover" 
                        data-object-position="top left"
                        v-bind:src="result.Fields.imageMobileUrl">
                </picture>
                <div v-else class="post-item__picture__wrap--placeholder">
                    <img class="img--placeholder" alt="placeholder image" src="/Static/img/placeholder_img_grid.jpg">
                </div>
            </a>

            <div class="slope">
                <div class="svg-container">
                    <svg>
                        <use xlink:href="/Aegon/Corporate/Static/2.13.0.0/dist/img/img.svg#icon-slope-right-flipped"></use>
                    </svg>
                </div>
            </div>
        </div>
    </div>

    <div class="post-item__text" v-scroll-animation="{ offsetWindow: 0, offsetElement: 'top' }">
        <hgroup>
            <h2 class="post-item__heading">
                <a v-bind:href="result.Fields.link">
                    {{ result.Title }}
                </a>
            </h2>

            <h3 class="post-item__subheading"
                v-if="!overviewModel.HideDate || !!result.Fields.author">
                <template v-if="result.Fields.authorBlockReference">
                    By <span rel="author">{{ result.Fields.author }}</span>
                    <span class="bullet">&bull;</span>
                </template>
                <template v-if="!overviewModel.HideDate">
                    <time pubdate="results.Fields.pubDate">{{ result.Fields.publishDateToDisplay }}</time>
                </template>
            </h3>
        </hgroup>

        <p class="post-item__desc"
            v-if="overviewModel.ShowGrid || result.Fields.linksToDisplay || overviewModel.ShowIntroOnListView">{{ result.Fields.overviewIntroduction }}</p>

        <div class="post-item__documents"
            v-if="result.Fields.linksToDisplay">
            <div class="container">
    <div class="related-downloads related-downloads--documents related-downloads--documents--columns">

        <div class="related-downloads__item"
            v-for="document in result.Fields.linksToDisplay"
            v-scroll-animation="{ offsetWindow: 0, offsetElement: 'middle' }">
            <div class="file-icon"
                v-bind:class="[document.Extension ? 'file-icon__' + document.Extension : 'file-icon__link']">
                <div class="svg-container">
                    <svg>
                        <use v-if="document.Extension" :xlink:href="iconPath + document.Extension"></use>
                        <use v-else :xlink:href="iconPath + 'link'"></use>
                    </svg>
                </div>
            </div>

            <p class="related-downloads__item__details">
                <a v-bind:href="document.Link" 
                    v-download 
                    v-bind:data-format="document.Extension | upperCase" 
                    v-bind:data-name="document.Text" 
                    class="file-name">{{ document.Text }}</a>
                <span class="file-details">
                    <span class="file-format" v-if="document.Extension">{{ document.Extension | upperCase }}</span>
                    <span class="file-size" v-if="document.Size">{{ document.Size }}</span>
                    <span class="file-date" v-if="result.Fields.publicationItemDate">{{ result.Fields.publicationItemDate }}</span>
                </span>
            </p>
        </div>
    </div>
</div>

        </div>

        <div class="post-item__actions"
            v-if="overviewModel.ShowGrid">
            <a v-bind:href="result.Fields.link" v-button class="button button--primary waves-effect">
                Read more
            </a>
        </div>
    </div>
