                <li>
                    <a href="https://www.woodplc.com/news">News</a>
                </li>
                <li>
                    <a href="https://careers.woodplc.com" target="_blank">Careers</a>
                </li>
                <li>
                    
                    <!-- Share Price -->
                    <div style="float:right;font-size:12px;margin-top:4px;">
              <div id="currentquote" style="float:left;"></div><div style="float:left;" class="shareprice-change" id="shareprice-change"></div>
              </div>
              <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
  crossorigin="anonymous"></script>
              <script>
              $.ajax({
                type: "GET",
                url: "https://tools.eurolandir.com/tools/pricefeed/xmlirmultiiso5.aspx?companyid=9951",
                dataType: "xml",
                success: function(xml) {
    
                var wgstockcurr = 0;    
                var wgstockclose = 0;

                $(xml).find("Quote").each(function (i) {
                if(i==0) {
                    wgstockcurr = $(this).text();
                    $( "#currentquote" ).html( $(this).text() );      
           
                    }
          
                });
      
      
      
                $(xml).find("Close").each(function (i) {
                      if(i==0) {
                        wgstockclose = $(this).text();
                        var wgstockchange = wgstockcurr - wgstockclose;
                        var rndstockchange = Number((wgstockchange).toFixed(2));  
			            var pctstockchange = wgstockchange / wgstockcurr * 100
            
			            $( "#shareprice-change" ).html( "GBX " ); 
			
			            if (wgstockchange > 0) {
                        $( "#shareprice-change" ).append( "<img src='https://www.woodplc.com/__data/assets/image/0017/123416/stockup.png' style='margin-right:3px;margin-top:1px;'/>" ); 
                        } else if(wgstockchange < 0) {
                        $( "#shareprice-change" ).append( "<img src='https://www.woodplc.com/__data/assets/image/0018/123417/stockdown.png' style='margin-right:3px;margin-top:1px;'/>" );  
            
                        }
                        $( "#shareprice-change" ).append( rndstockchange + " (" + Number(pctstockchange).toFixed(2) + "%)" );
                      }
          
                  });
      
                }
            });
            </script>
              <!-- Share Price -->
