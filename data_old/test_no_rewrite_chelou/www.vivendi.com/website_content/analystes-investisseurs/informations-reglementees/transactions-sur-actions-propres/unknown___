	<script>
	jQuery(document).ready(function()
{
 jQuery.address.change(function(event)
 {
 if ((typeof(event.value) != 'undefined') && (event.value != '') && (event.value != null))
 {
 var $selector = jQuery('.widget-presentations .switch-menu a[href="#!' + event.value + '"]');
 if ($selector.length > 0)
 {
 var year = $selector.data('year');
 var $item = jQuery('#presentations-' + year);
 if ($item.hasClass('js-hidden') === true)
 {
 jQuery('.widget-presentations .switch-menu a').removeClass('current');
 $selector.addClass('current');
 jQuery('.widget-presentations .year').addClass('js-hidden').hide();
 $item.removeClass('js-hidden').show();
 jQuery('.eq-col').attr('style', '');
 jQuery('.eq', $item).teqrowcolizr(
 {
 classChildSelector: '.eq-col',
 classRowSelector: 'rowcolizr-row',
 classColSelector: 'rowcolizr-col'
 });
 }
 }
 }
 });
});	jQuery(document).ready(function() {
 jQuery.address.change(function(event) {
 var show_first = true;
 
 if ((typeof(event.value) != 'undefined') && (event.value != '') && (event.value != null)) {
 var deep = event.value;
 var slug = deep.substr(1);
 
 var $selector = jQuery('.menu-contacts ul li a[data-slug="' + slug + '"]');
 if ($selector.length == 1) {
 jQuery('.item-contacts').hide();
 jQuery('#contact_' + slug).show();
 // scroll automatique si mobile
 if (jQuery(window).width() < 743) {
 jQuery('html,body').animate({scrollTop: jQuery('#contact_' + slug).offset().top - 10});
 }
 show_first = false;
 }
 }
 
 if (show_first) {
 jQuery('.item-contacts:eq(0)').show();
 }
 });
});	jQuery(document).ready(function() {
 jQuery.address.change(function(event) {
 if ((typeof(event.value) != 'undefined') && (event.value != '') && (event.value != null)) {
 var $selector = jQuery('.widget-results .switch-menu a[href="#!' + event.value + '"]');
 
 if ($selector.length > 0) {
 var year = $selector.data('year');
 var $item = jQuery('#results-' + year);
 
 if ($item.hasClass('js-hidden') === true) {
 jQuery('.widget-results .switch-menu a').removeClass('current');
 $selector.addClass('current');
 
 jQuery('.widget-results .year').addClass('js-hidden').hide();
 $item.removeClass('js-hidden').show();
 
 jQuery('.eq-col').attr('style', '');
 jQuery('.eq', $item).teqrowcolizr({
 classChildSelector: '.eq-col',
 classRowSelector: 'rowcolizr-row',
 classColSelector: 'rowcolizr-col'
 });
 }
 }
 }
 });
});	</script>
