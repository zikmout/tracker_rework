<script type="text/javascript" >
/* <![CDATA[ */
 var domLoadFunc = function() { 
   var t1 = new Date().getTime(); 

	     
var iTimeOpen = 2500;
var oClearTime = {};

var fnMenuActive = function() {
    jQuery("ul.main-menu > li.dropdown.open").removeClass("open");
    fnResetTimers();
    
    var item = jQuery(this).addClass("open");
    item.one("mouseleave", function() {
        oClearTime[item.data("catid")] = setTimeout(function() {
            item.removeClass("open");        
        }, iTimeOpen);
    });
}

var fnResetTimers = function() {
    for (var key in oClearTime) {
        console.log(">>", key, " >> clearTimeout");
        clearTimeout(oClearTime[key]);
        delete oClearTime[key];
    }
}

jQuery("ul.main-menu").find("li.dropdown").mouseenter(fnMenuActive);   

jQuery(".natixis-menu > li > .dropdown-arrow").click(function(e) {
    e.stopPropagation();
    
    var jqArrow = jQuery(this);  
    var isOpen = jqArrow.hasClass("open");
    var menu = jqArrow.data("menu");
    var submenu = jqArrow.data("submenu");
    
    var jqList = jQuery("ul[data-menu=" + menu + "]"); 
    var jqItem = jQuery("li[data-menu=" + submenu + "]"); 
    
    jqArrow.toggleClass("open");
    jqList.toggleClass("open-menu");
    jqItem.toggleClass("opened");
    
    if(isOpen) {
        jQuery("ul[data-menu=" + submenu + "]").slideToggle();
        jqList.children("li").not(jqItem).slideToggle();
        return;
    }
    
    var jqMenu = jQuery(".dropdown.open").unbind();
    var jqSubMenu = jqMenu.siblings("ul");   
    
    jqList.children("li").not(jqItem).slideToggle();
    jQuery("ul[data-menu=" + submenu + "]").slideToggle(function() {
        jqMenu.mouseenter(fnMenuActive);  
    });
});

jQuery(".natixis-menu>li>.dropdown-back").click(function(e) {
    e.stopPropagation();

    var jqArrow = jQuery(this);  
    var isOpen = jqArrow.hasClass("open");
    var menu = jqArrow.data("menu");
    var submenu = jqArrow.data("submenu");
    
    var jqList = jQuery("ul[data-menu=" + menu + "]"); 
    var jqItem = jQuery("li[data-menu=" + submenu + "]"); 
    var jqNext = jQuery("span.dropdown-arrow.open[data-menu=" + menu + "]");
    
    jqNext.toggleClass("open");
    jqItem.toggleClass("opened");
    jqList.toggleClass("open-menu");
    
    if(isOpen) {
        jQuery("ul[data-menu=" + submenu + "]").slideToggle();
        jqList.children("li").not(jqItem).slideToggle();
        return;
    }
    
    jqList.children("li").not(jqItem).slideToggle();
    jQuery("ul[data-menu=" + submenu + "]").slideToggle();
});

jQuery(".natixis-submenu>li>.dropdown-arrow").click(function(e) {
    e.stopPropagation();
  
    var jqArrow = jQuery(this);  
    var isOpen = jqArrow.hasClass("open");
    var menu = jqArrow.data("menu");
    var submenu = jqArrow.data("submenu");
    
    var jqList = jQuery("ul[data-menu=" + menu + "]"); 
    var jqItem = jQuery("li[data-menu=" + submenu + "]"); 
    
    jqArrow.toggleClass("open");
    
    jQuery("ul[data-menu=" + submenu + "]").slideToggle();
});


	     
    (function($) {
    
        menu = window.location.toString();  
        var arr = menu.split('/');
        selectedMenu = arr[arr.length - 1];
        
        $(document).ready(function(){   
            $('a').removeClass("active-menu");
            if(selectedMenu) {
                if($("a[href='"+selectedMenu+"']")) {
                    $("a[href='"+selectedMenu+"']").addClass("active-menu");
                }
            }
        });
    })(jQuery);

	     
jQuery(document).ready(function(){
    jQuery('[data-toggle="tooltip"]').tooltip(); 
});

	     
	jQuery('#year').on('change', function() {	
	    var val = jQuery(this).val();
	    var cat_href = val.split("|");
	    var href = cat_href[1]; 
	    window.location.href = href;
	});
	

	     
    
    /* Hamburger Button */
    
    jQuery(".cmn-toggle-switch").on('click', function() {
    	jQuery('#accordion_1_mobile').toggleClass('toggled');
		jQuery('#accordion_1_mobile').find(".panel-collapse.collapse.in").each(function() {
			jQuery(this).removeClass('in');
		});
	});

    

	     

jQuery(document).ready(function(){
	jQuery("iframe").addClass('embed-responsive-item');
	jQuery("iframe").wrap('<div class="embed-responsive embed-responsive-16by9"></div>');
});


jQuery(document).ready(function(){
	jQuery("table").addClass('table');
	jQuery("table").wrap('<div class="table-responsive"></div>');
}); 

	     
    jQuery('body').append('<div class="printHide hidden-xs hidden-sm"><a href="https://www.natixis.com/natixis/jcms/ala_5525/en/share-buyback-programs" class="top_link_lg" style="display: none;" title="Top"><img src="plugins/RWDCorePlugin/images/arrow_top.png" alt="" /></a></div>');
    jQuery('.top_link_lg').css({
        'position'              :   'fixed',
        'right'                 :   '17.5%',
        'bottom'                :   '20px',
        'opacity'               :   '0.7',
        'z-index'               :   '2000',
    });
    jQuery('.top_link_lg').on('click touchstart', function (event) {
        event.preventDefault();
        event.stopPropagation();
        jQuery('html, body').stop().animate({
            scrollTop: 0
        }, 500);
    });
    
	jQuery('body').append('<div class="printHide hidden-md hidden-lg"><a href="https://www.natixis.com/natixis/jcms/ala_5525/en/share-buyback-programs" class="top_link_sm" title="Top"><img src="plugins/RWDCorePlugin/images/arrow_top.png" alt="" /></a></div>');
    jQuery('.top_link_sm').css({
        'position'              :   'fixed',
        'right'                 :   '10px',
        'bottom'                :   '20px',
        'opacity'               :   '0.7',
        'z-index'               :   '2000',
    });
    jQuery('.top_link_sm').on('click touchstart', function (event) {
        event.preventDefault();
        event.stopPropagation();
        jQuery('html, body').stop().animate({
            scrollTop: 0
        }, 500);
    });
    
    var windowWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    
    jQuery(window).scroll(function(){    
    	if(jQuery(window).scrollTop() > 0) {
    		if(windowWidth <= 766) {
				jQuery(".top_link_sm").fadeIn(500);	
			} else {
				jQuery(".top_link_lg").fadeIn(500);
			}    		
		} else {
			if(windowWidth <= 766) {
				jQuery(".top_link_sm").fadeOut(500);
			} else {
				jQuery(".top_link_lg").fadeOut(500);
			}
		}
    });
    
    jQuery(document).ready(function() {    	
		jQuery(".top_link_lg").hide();
		jQuery(".top_link_sm").hide();	
	});

	     
