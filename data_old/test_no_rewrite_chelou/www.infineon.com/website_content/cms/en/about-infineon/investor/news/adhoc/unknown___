    var consentCookieName = 'cookie_consent_1';
    var cookieCustomEventName = 'cookieConsentCheck';
    var consentCookieExpireDays = 2 * 365 + 1; // 2 years + 2020 is a leap year
    var googleAnalyticsId = window.location.host.indexOf('infineon.com') > 0 ? 'UA-143787472-1' : 'UA-143787472-2';
    var cookieConsentCookieModalReady = false;

    if (window.preventTrackingOnPage) {
        window['ga-disable-' + googleAnalyticsId] = true;
    } else {
        var cookiesAlreadyConfirmedMatch = document.cookie.match(new RegExp('(^| )' + consentCookieName + '=([^;]+)'));
        if (cookiesAlreadyConfirmedMatch) {
            var cookiesAlreadyConfirmed = cookiesAlreadyConfirmedMatch[2];
        }
        if (navigator.cookieEnabled && navigator.doNotTrack != "1" && document.cookie.indexOf(consentCookieName) == -1) {
            // leave tracking enabled and show the cookie dialog
            var cookieConsentCheckedEvent = new CustomEvent(cookieCustomEventName, {
                bubbles: true,
                detail: {cookieModalCanBeShown: true}
            });
            var checkCookieModalIsInitialized = setInterval(function () {
                if (typeof window.vi.cookieModal === 'function' && cookieConsentCookieModalReady) {
                    document.dispatchEvent(cookieConsentCheckedEvent);
                    clearInterval(checkCookieModalIsInitialized);
                }
            }, 1000);
        } else if (cookiesAlreadyConfirmed == 'true') {
            // leave tracking enabled and don't open cookie modal dialog
        } else {
            if (navigator.doNotTrack == '1') {
                // we need to set cookie_consent cookie to false immediately to prevent GTM to init GoogleAnalytics tracking.
                // based on vi.helpers.setCookie, but vi.helpers is not available yet
                var cookieExpireDate = new Date();
                cookieExpireDate.setTime(cookieExpireDate.getTime() + (consentCookieExpireDays * 24 * 60 * 60 * 1000));
                document.cookie = consentCookieName + '=false; ' + 'expires=' + cookieExpireDate.toUTCString() + ';domain=;path=/';
            }

            window['ga-disable-' + googleAnalyticsId] = true;
            // remove all marketing cookies and disable GA tracking (see below) and hide the cookie dialog"
            var cookieConsentCheckedEvent = new CustomEvent(cookieCustomEventName, {
                bubbles: true,
                detail: {cookieModalCanBeShown: false}
            });
            var checkCookieModalIsInitialized = setInterval(function () {
                if (typeof window.vi.cookieModal === 'function' && cookieConsentCookieModalReady) {
                    document.dispatchEvent(cookieConsentCheckedEvent);
                    clearInterval(checkCookieModalIsInitialized);
                }
            }, 1000);
        }
    }
</script>
