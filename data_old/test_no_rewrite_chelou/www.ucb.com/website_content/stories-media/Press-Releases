	        <script type="text/javascript">

            var pagingLinkText = 'page';
            var filterBasePath = '/stories-media/Press-Releases';
            var filterHasChanged = false;
            var dateStringFormat = 'dd/mm/yy';
            var buttonImageURL = 'media/js/libs/jqueryui/theme/ui-lightness/images/datepicker-icon.gif';

            $(document).ready(function() {
                var datepickerOptions = {
					dateFormat: dateStringFormat,
					showOn: 'both',
					buttonImageOnly: true,
					buttonText: '',
					buttonImage:buttonImageURL,
					dayNamesMin: [ "Su", "Mo", "Tu", "We", "Th", "Fr", "Sa" ],
					monthNames: [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" ]				};
                $('.dp').datepicker(datepickerOptions);
                $('input, select').on('change', handleFilterChange);
            });

            function handleFilterChange() {
                filterHasChanged = true;
            }

            function sub(page) {
                if (filterHasChanged === true) {
                    page = 1;
                }
                var path = filterBasePath + '/' + pagingLinkText + '/' + page;
                path = path.replace('//', '/');
                $('#filterBox').attr('action', path).submit();
            }

        </script><!-- start filter box --><div class="form-block filterBox">
