    <script>
    window.CookieCommand = window.CookieCommand || {};
    window.CookieCommand.Commands = {};

        window.CookieCommand.Commands["googleAnalytics"] = {"triggerOn" : function() {var gaProperty = 'UA-122967298-1'; var disableStr = 'ga-disable-' + gaProperty;  var disableStr = 'ga-disable-' + gaProperty;
if (document.cookie.indexOf(disableStr + '=true') > -1) {
 window[disableStr] = true;
} document.cookie = disableStr + '=false; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/';
 window[disableStr] = false;},
"triggerOff": function() {var gaProperty = 'UA-122967298-1'; var disableStr = 'ga-disable-' + gaProperty; document.cookie = disableStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/';
 window[disableStr] = true;}}
;
        window.CookieCommand.Commands["sitecoreAnalytics"] = {"triggerOn" : function() {Cookies.set('sitecoreAnalytics', 'on', { expires: 182 }); },
"triggerOff": function() {console.log('Cookie will be deleted with next request')}}
;
        window.CookieCommand.Commands["facebookMarketing"] = {"triggerOn" : function() {Cookies.set('facebookMarketing', 'on', { expires: 182 }); }, "triggerOff": function() {console.log('Cookie will be deleted with next request')}};
        window.CookieCommand.Commands["linkedinMarketing"] = {"triggerOn" : function() {Cookies.set('linkedinMarketing', 'on', { expires: 182 }); }, "triggerOff": function() {console.log('Cookie will be deleted with next request')}};
        window.CookieCommand.Commands["CustomerSurveyExpire"] = {"triggerOn" : function() {Cookies.set('customerSurveyExpire', 'on', { expires: 182 }); }, "triggerOff": function() {console.log('Cookie will be deleted with next request')}};

    window.CookieCommand.Autofire = function() {
        for (var key in this.Commands) {
            console.log('KEYS:' + key);
            var cookieVal = Cookies.get(key);

            if (typeof cookieVal === 'undefined') {
                console.log('Bin nicht da...');
                Cookies.set(key, 'on', { expires: 182 });
                if (this.Commands[key] &&
                    this.Commands[key].triggerOn &&
                    typeof this.Commands[key].triggerOn === 'function') {
                    this.Commands[key].triggerOn();
                }

            } else if (cookieVal === 'on') {
                console.log('Bin ON...');
                if (this.Commands[key] &&
                    this.Commands[key].triggerOn &&
                    typeof this.Commands[key].triggerOn === 'function') {
                    this.Commands[key].triggerOn();
                }
            } else {
                console.log('Bin OFF...');
                if (this.Commands[key] &&
                    this.Commands[key].triggerOn &&
                    typeof this.Commands[key].triggerOff === 'function') {
                    this.Commands[key].triggerOff();
                }
            }
        }
    }

    if (typeof Cookies.get('cookieAgreement') !== 'undefined') {
        window.CookieCommand.Autofire();
    }
</script>


</body>
