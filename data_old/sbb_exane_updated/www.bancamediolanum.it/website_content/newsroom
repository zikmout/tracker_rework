                }
            });
        })(jQuery);
    </script>
          

        	<script>
            	$ = jQuery;
                var lastUpdate = "11/08/2016 23:00:00";
                lastUpdate = lastUpdate.substr(0, 10);
                var day = lastUpdate.substr(0, 2);
                var month = lastUpdate.substr(3, 2);
                lastUpdate = lastUpdate.replace(lastUpdate.substring(3, 5), day);
                lastUpdate = lastUpdate.replace(lastUpdate.substring(0, 2), month);
            	jQuery("#lastUpdate").append("Ultimo aggiornamento: " + lastUpdate);
                jQuery('#lastUpdate').each(function () {
                    if (!jQuery(this).text().match(/^\s*$/)) {
                        jQuery(this).insertAfter(jQuery(this).next('#copyright'));
                    }
                });
                
            </script>

        
