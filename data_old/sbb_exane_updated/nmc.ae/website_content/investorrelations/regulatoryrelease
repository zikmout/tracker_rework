<div class="newsletter padding margin-vertical">
    <h6>Newsletter Sign Up</h6>
<form action="/Home/NewsletterSubscription" id="frmNewsLetter" method="post"><input name="__RequestVerificationToken" type="hidden" value="BwknuHUpQG_QVtn12QvNX9bsDvEPiJ9AsKR5W6OcScHAwouIsVmZE0tYQcHoOXBtUzI_Z3sYJVIodVttnHtl_UTdzdJ6ONuFy1yE1sTKYWY1" />        <div class="input-group input-group-sm">
            <input autocomplete="off" class="form-control" id="NewsLetterEmail" maxlength="50" name="NewsLetterEmail" placeholder="enter email id" type="text" value="" />

            <div class="input-group-btn">
                <button class="btn btn-primary" id="anchsubmit" type="submit">Subscribe</button>
            </div>
        </div>
        <span id="NewsLetterEmailvalid" class="field-validation-error" style="color:red"></span>
        <div id="divMsg" style="color:green;"> </div>
</form></div>
<script type="text/javascript">

    $(function () {

        $("#frmNewsLetter").submit(function (e) {
            e.preventDefault();
            $("#divMsg").html('');
            $("#NewsLetterEmailvalid").html('');

            if ($.trim($("#NewsLetterEmail").val()) == "") {
                $("#NewsLetterEmailvalid").html('Email is required')
                return false;
            }
            else {
                if (!checkEmaiformat($.trim($("#NewsLetterEmail").val()))) {
                    $("#NewsLetterEmailvalid").html('Invalid email')
                    return false;
                }
                else {
                    $.ajax({
                        url: "/Home/CheckEmail",
                        data: { NewsLetterEmail: $.trim($("#NewsLetterEmail").val()) },
                        success: function (data) {
                            if (data == false) {
                                $("#NewsLetterEmailvalid").html('You have already subscribed our newsletter');
                                $("#divMsg").html('');
                                return false;
                            }
                            else {
                                $("#NewsLetterEmailvalid").html('');
                                AddNewsLetter();
                            }
                        }
                    })

                }
            }

        });

        

    });
    function AddNewsLetter() {

        var newslettertokenform = $('#frmNewsLetter');
        var newslettertoken = $('input[name="__RequestVerificationToken"]', newslettertokenform).val();

        $('#anchsubmit').attr("disabled", "disabled");

        var formData = new FormData();
        formData.append("NewsLetterEmail", $.trim($("#NewsLetterEmail").val()));
        formData.append("__RequestVerificationToken", newslettertoken);

        $.ajax({
            url: '/Home/NewsletterSubscription',
            type: "POST",
            data: formData,
            dataType: 'json',
            success: function (result) {
                $('#anchsubmit').removeAttr("disabled");

                $("#NewsLetterEmail").val("");
                $("#divMsg").html("Newsletter subscribed successfully");
            },
            error: function (result) {
                $('#anchsubmit').removeAttr("disabled");
                alert("Something went wrong. Please try later");
            },
            cache: false,
            contentType: false,
            processData: false
        });

    }
    function checkEmaiformat(str) {

        var filter = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/
        if (filter.test(str))
            testresults = true
        else {
            testresults = false
        }
        return (testresults)
    }
</script>

