<script>

    var showTag = null;
    var showType = null;
    var showYear = null;
    var showMonth = null;
    var showPrice = null;
    var showFeed = null;

    function showHide() {
    	if (showTag === null && showType === null && showYear === null && showPrice === null) {
    		jQuery('.sNew').show();
    		jQuery('.sNewDoc').show();
    		jQuery('.tagInBox').show();
    	} else {
    		var tags = '';
    		jQuery('.sNew').hide();
            jQuery('.sNewDoc').hide();
    		jQuery('.tagInBox').hide();
    		if (showTag != null)
    			tags = tags + '.' + showTag;
            if (showType != null)
                tags = tags + '.' + showType;
    		if (showYear != null)
    			tags = tags + '.' + showYear;
        if (showMonth != null)
          tags = tags + '.' + showMonth;
    		if (showPrice != null)
    			tags = tags + '.' + showPrice;
    		jQuery(tags).show();
    	}
    }

    function selectTag(object) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	if (object[0].innerText == 'Tag')
    		showTag = null;
    	else
    		showTag = 'tag_' + object[0].value;
    	showHide();
    }

    function selectType(object) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
        if ( object[0].innerText == jQuery("#selType")[0][0].innerHTML )
    		showType = null;
    	else
    		showType = 'tag_' + object[0].value;
    	showHide();
    }

    function selectYear(object) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	if ( object[0].innerText == jQuery("#selYear")[0][0].innerHTML ) {
          showYear = null;
          showMonth = null;
          jQuery("#selMonth").val("").change();
          jQuery("#selMonth").attr('disabled', 'disabled');
          jQuery("#uniform-selMonth").addClass('disabled');
        } else {
          showYear = 'tag_' + object[0].innerText;
          jQuery("#selMonth").removeAttr('disabled');
          jQuery("#uniform-selMonth").removeClass('disabled');
        }
    	showHide();
    }

    function selectMonth(object) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	if ( object[0].value == jQuery("#selMonth")[0][0].value )
    		showMonth = null;
    	else
    		showMonth = 'tag_' + object[0].value;
    	showHide();
    }

    function filterPrice(el) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	if (el.checked) {
    		showPrice = 'tag_true';
    		showHide();
    	} else {
    		showPrice = null;
    		showHide();
    	}
    }

    function filterFeed(el) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	if (el.checked) {
    		showFeed = 'tag_true';
    		showHide();
    	} else {
    		showFeed = null;
    		showHide();
    	}
    }

    function clickTag(tagName) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	showTag = 'tag_' + tagName;
    	showHide();
    	jQuery('html,body').animate({
    		scrollTop: jQuery("#elNews").offset().top - 50
    	}, 'slow');
    	jQuery("#selTag").val(tagName.trim()).change();
    }

    function clickType(typeName) {
        showAllDocs(9);
    	showType = 'tag_' + typeName;
    	showHide();
    	jQuery('html,body').animate({
    		scrollTop: jQuery("#elNews").offset().top - 50
    	}, 'slow');
    	jQuery("#selType").val(typeName.trim()).change();
    }

    function displayAll(el) {
        if( window.location.pathname == "/newsroom")
            showAllNews(9);
        else
            showAllDocs(9);
    	jQuery("#selTag").val('').change();
    	jQuery("#selType").val('').change();
    	jQuery("#selYear").val('').change();
    	jQuery('.rewFilter a').css({
    		"font-weight": "bold"
    	});
    	showTag = null;
    	showType = null;
    	showYear = null;
      showMonth = null;
    	showPrice = null;
      showFeed = null;
    	$(".noUniform").attr('checked', false);
    	showHide();
        jQuery('.readmored').hide();
    }

  	function filterURLParameter() {
    }

    document.getElementById("selMonth").disabled = "disabled";
</script>
<div class="row100 boxcontent-boximmagine" style="min-height: 0">
  
    
      <div class="row100  h-images">
    <div class="sized-content">
      <section class="module module box-shadow">
            
        <div class="boxcontent-boximmagine-box ">
                    
                    
        </div>
      </section>
        <div class="space-bottom boximmagine"></div>
        </div>
      </div>
</div>					</div>
				</div>
                <div style="clear:both;"></div>
<footer>
