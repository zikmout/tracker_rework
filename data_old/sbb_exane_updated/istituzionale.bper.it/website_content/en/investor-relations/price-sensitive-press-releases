<script>
$(document).ready(function(){
	setFooterHeight();
});
$(window).load(function(){
	setFooterHeight();
});
$(window).resize(function(){
	setFooterHeight();
});

function setFooterHeight(){
	var h  = 0;
	$(".ist-footer .footer-container .dark-footer").css("height","auto");
	$(".ist-footer .footer-container>div").css("height","auto");
	$(".ist-footer .footer-container>div").each(function(){
		if($(this).height()>h){
			h = $(this).height();
		}
	})
