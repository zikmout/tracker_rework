        if (typeof(libeUserInfo["softwarer_blocker"]) != "undefined") {
            xt_multc += "&x8=" + libeUserInfo["softwarer_blocker"]
        }

        if (window.xtparam==null) { window.xtparam = ''; }
        window.xtparam += "&ptype="+xt_pagetype+"&ac="+xt_ac+"&an="+xt_an+xt_multc+"&ati="+xt_ati;
    </script>
