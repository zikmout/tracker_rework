        <div id="dialogexitPopup" style="display: none; overflow-x: hidden;" align="center">
            <div class="popup-container">
                
            </div>
            <div class="popup-control">
                <input type="button" class="button" id="continuepopup" value="Continue" />
                <input type="button" class="button" id="closepopup" value="Cancel" />
            </div>
        </div>

        <script type="text/javascript">
            function dialogexitPopup() {
                $("#dialogexitPopup").dialog({
                    modal: true,
                    autoOpen: true,
                    width: "auto",
                    height: "auto",
                    top: "100px",
                    closeOnEscape: true,
                    draggable: false
                });
                $(".ui-widget-overlay")
                    .css("z-index", "99998")
                    .css("background-image", "none");

                $(".ui-dialog").css("z-index", "99999");
                $(".ui-dialog-titlebar").css("display", "none");
                $(".ui-widget-content").css("background", "white");

            }
        </script>

        <script type="text/javascript">
            // Open external links in a popup modal notice
            $(window).on('load', function () {
                var win = window.location.href;
                if (win.includes("/australia/") || win.includes("/new-zealand/")) {
                    $.expr[":"].external = function (a) {
                        var winhref = window.location.href;
                        var auswin = winhref.includes("/australia/");
                        var nzwin = winhref.includes("/new-zealand/");
                        var aussite = a.href.includes("/australia/");
                        var nzsite = a.href.includes("/new-zealand/");
                        return auswin != aussite || nzwin != nzsite;
                    };
                    $("a:external").addClass("ext_link");
                    $(function () {
                        $('a.ext_link').click(function () {
                            var au = this.href.includes("/australia/");
                            var nz = this.href.includes("/new-zealand/");
                            var aunzsite = au || nz;
                            if (aunzsite == false) {
                                event.preventDefault();
                                dialogexitPopup();
                                var url = this.href
                                $('#continuepopup').click(function () {
                                    window.location.href = url;
                                });
                                $('#closepopup').click(function () {
                                    $("#dialogexitPopup").dialog('close');
                                });
                            }
                        });
                    });
                }
            });
        </script>


</html>
