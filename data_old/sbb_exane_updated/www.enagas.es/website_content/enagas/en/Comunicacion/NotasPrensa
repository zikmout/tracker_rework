<script type="text/javascript">

function callHtmlTabs(channelId, channelSelHijo){
	var language = 'es';
	var url_string = "/web-corporativa-ext-templating/webcorp/plantillaDistributiva/getHtmlTabs?channelId=".concat(channelId).concat("&channelSelHijo=").concat(channelSelHijo).concat("&language=").concat(language);

	$.ajax({
		  url:url_string,
		  type: 'post',
		  dataType: 'html',
		  contentType: 'Content-type: text/plain; charset=iso-8859-1',
		  beforeSend: function(jqXHR) {
		    jqXHR.overrideMimeType('text/html;charset=iso-8859-1');
		  },
		  success: function (data) {
		  //$("h2:first").parent().after(data);
		  $("h2:first").after(data);
	  },
	});
}

$(document).ready(function(){
	if(false){
		var channelId = '';
		var channelSelHijo = '';
		callHtmlTabs(channelId, channelSelHijo);
	}
});  

</script>



	
	

