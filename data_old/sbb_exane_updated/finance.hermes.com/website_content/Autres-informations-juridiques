	<script>
	var cookie = (function (){
	var name =  "infocookie";
	var value = "accepted";
	var duration = 86400000;
	var rg = new RegExp("^(?:.*; )?"+name+"=([^;]+)(?:.*)?$");
	var localset = (document.cookie.match(rg)||[,null])[1];
return {
	name: name , 
	value : value ,
	duration : duration , 
	rg :rg , 
	localset : localset,
	stack : new Array(),
	isset : false,
	listener : function(Event) {


		if( Event.target.tagName == 'A' &&  Event.target.href.match(/Cookies/)){
			return true;
		}
		var req = new XMLHttpRequest();
		req.open('GET', '/cookie/accept', false); 
		req.send(null);
	    acceptCookie(); 

  }
 };
})();
		//createCookie(cookie.name,cookie.value,cookie.duration);
	</script>
	

	

