	var rangeproductJS = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/ranges-products.js?currentUri=/en/investor-relations/arkema-share/share-buybacks/2020/index.html&currentSiteRoot=/sites/global&locale=en&isingalaxy=true";
