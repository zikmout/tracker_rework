<script type="text/javascript">
    jQuery(document).ready(function () {

        var currentLang = jQuery.cookie("BMLang");

        if (currentLang === "it") {
            jQuery('#localization-desktop input[type="image"].destkopITSubmit').show();
            jQuery('#localization-desktop input[type="image"].destkopITSubmit').on('click', function (event) {
                event.preventDefault();
                setLang(jQuery('#localization-desktop input[type="image"].destkopENGSubmit').val(), true);
            });
        } else if (currentLang === "en") {
            jQuery('#localization-desktop input[type="image"].destkopENGSubmit').show();
            jQuery('#localization-desktop input[type="image"].destkopENGSubmit').on('click', function (event) {
                event.preventDefault();
                setLang(jQuery('#localization-desktop input[type="image"].destkopITSubmit').val(), true);
            });
        }


        /* bck #localization-desktop
           jQuery('#localization-desktop span input[type="image"]').each(function(){
               if(jQuery(this)[0].value.toLowerCase()===currentLang.toLowerCase()){
                   jQuery(this)[0].className='current';
                   jQuery(this)[0].style.visibility='visible';
                   jQuery('#localization-desktop input[type="text"]')[0].style.visibility='visible';
                   jQuery('#localization-desktop input[type="text"]')[0].style.opacity=1;
               } else{
                jQuery(this)[0].className='other';
                jQuery(this)[0].style.visibility='hidden';
                jQuery(this)[0].style.opacity=0;
            }
            });
            jQuery('#localization-desktop span input[type="image"].current').on('click', function(event){
               event.stopPropagation();
               event.preventDefault();
               if(jQuery('#localization-desktop input[type="text"]').outerWidth()>45){ // dimensione minima da universal.css
                   setLang(jQuery('#localization-desktop span input[type="image"].current')[0].value, true);
               }else{
                   jQuery('#localization-desktop input[type="text"]').focus();
               }
           });
           jQuery('#localization-desktop span input[type="image"].current').on('click', function(event){
               event.preventDefault();
               setLang(jQuery('#localization-desktop span input[type="image"].other')[0].value, true);
           });
           jQuery('#localization-desktop span input[type="image"]').each(function(){
               if(jQuery(this)[0].value.toLowerCase()===currentLang.toLowerCase()){
                   jQuery(this)[0].className='current';
                   jQuery(this)[0].style.visibility='visible';
                   jQuery('#localization-desktop input[type="text"]')[0].style.visibility='visible';
                   jQuery('#localization-desktop input[type="text"]')[0].style.opacity=1;
               } else{
                jQuery(this)[0].className='other';
                jQuery(this)[0].style.visibility='hidden';
                jQuery(this)[0].style.opacity=0;
            }
        });

           jQuery('#localization-desktop input[type="text"]').focus(function(){
               jQuery('#localization-desktop span input[type="image"].other').attr("style","visibility:visible;");
               jQuery('#localization-desktop span input[type="image"].other').attr("style","opacity:1;");
           });

           jQuery('#localization-desktop input[type="text"]').blur(function(){
               jQuery('#localization-desktop span input[type="image"].other').attr("style","visibility:hidden;");
               jQuery('#localization-desktop span input[type="image"].other').attr("style","opacity:0;");
           });

           bck localization-desktop */

        jQuery('#new-localization-desktop span input[type="image"]').each(function () {
            if (jQuery(this)[0].value.toLowerCase() === currentLang.toLowerCase()) {
                this.className = 'current';
                jQuery(this).closest('span').prev().addClass('current');
            } else {
                this.className = 'other';
                jQuery(this).closest('span').prev().addClass('other');
            }
        });

        jQuery("#search-desktop").validate({
            rules: {
                desktopSearchText: {
                    /*minlength: 3,
                    maxlength: 256,
                    required:true*/
                }
            },
            messages: {
                /*"desktopSearchText": {
                    maxlength: jQuery.validator.format("Massimo 256 caratteri."),
                    minlength: jQuery.validator.format("Minimo 3 caratteri."),
                    required: ""
                }*/
            },
            submitHandler: function (form) {
                document.location = ('/ricerca?q=' + document.getElementById('desktopSearchText').value + '&filtro=tutti&p=1');
            }
        });

        jQuery('#search-desktop input[type="submit"], #search-desktop button[type="submit"]').on('click', function (event) {
            console.log("BEFORE IF");
            if (jQuery('#search-desktop input[type="text"]')[0].value === '') {
                event.preventDefault();
                jQuery('#search-desktop input[type="text"]').focus();
                console.log("IF");
            }
            console.log("AFTER IF");
        });

        jQuery('#new-localization-desktop span input[type="image"]').on('click', function (event) {
            event.stopPropagation();
            event.preventDefault();
            setLang(this.value, true);
        });

        /*jQuery('#new-localization-desktop input[type="image"].other').on('click', function(event){
            event.stopPropagation();
            event.preventDefault();
            setLang(jQuery('#new-localization-desktop input[type="image"].other')[0].value, true);
        });*/

        var linkFamilyBanker = jQuery("#myIdCampagna").attr("href");
        if (linkFamilyBanker != undefined) {
            jQuery("#myIdCampagna").attr("href", linkFamilyBanker + "?idCampagna=" + myIdCampagna);
        }
        var linkFamilyBankerDirect = jQuery("#myIdCampagnaDirect").attr("href");
        if (linkFamilyBankerDirect != undefined) {
            jQuery("#myIdCampagnaDirect").attr("href", linkFamilyBankerDirect + "?idCampagna=" + myIdCampagna);
        }

        jQuery('#localization-mobile input[type="image"].eng').on('click', function (event) {
            event.preventDefault();
            setLang('en', true);
        });

        jQuery('#localization-mobile input[type="image"].it').on('click', function (event) {
            event.preventDefault();
            setLang('it', true);
        });

        if (jQuery('.sNewDoc').length > 0) {

            var numDocToShow = "";

            if (jQuery('.sNewDoc').length > 9) {
                numDocToShow = 9;
            } else {
                numDocToShow = jQuery('.sNewDoc').length - 1;
            }
            jQuery(document).ready(function () {

                if (numDocToShow == 9) {
                    jQuery(jQuery('.sNewDoc')[numDocToShow]).nextAll(jQuery('.sNewDoc')).hide();
                    jQuery(jQuery('.sNewDoc')[numDocToShow])[0].style.marginBottom = "70px";
                    jQuery('<div class="sNewDoc readmored" style="text-align: center; border: 0; margin-top:-50px; margin-bottom:50px;"> <div style="display: inline-block;"><a class="button-custom-azzurro" style="margin-bottom: 0px;margin-top: 10px;" href="#!" onclick="showAllDocs(' + numDocToShow + ');">Leggi tutto</a><p style="top: 0px;left: 35px;">(' + (jQuery('.sNewDoc').length - numDocToShow - 1) + ' documenti)</p></div> </div>').insertAfter(jQuery('.sNewDoc')[numDocToShow]);
                }
            });
        }

        if (jQuery('.sNew').length > 0) {

            var numNewsToShow = "";

            if (jQuery('.sNew').length > 9) {
                numNewsToShow = 9;
            } else {
                numNewsToShow = jQuery('.sNew').length - 1;
            }
            /*
            jQuery(document).ready(function() {
                */
            if (numNewsToShow == 9) {
                jQuery(jQuery('.sNew')[numNewsToShow]).nextAll(jQuery('.sNew')).hide();
                jQuery(jQuery('.sNew')[numNewsToShow]).nextAll(jQuery('.tagsContainer')).hide();
                if (window.matchMedia("(min-width: 641px)").matches == true) {
                    jQuery(jQuery('.sNew')[numNewsToShow]).next(jQuery('.tagsContainer')).show();
                } else {
                    jQuery('.content-wrap')[0].style.marginBottom = "60px";
                }
                jQuery('<div class="sNew readmored" style="text-align: center; border: 0;"> <div style="display: inline-block;"><a class="button-custom-azzurro" style="margin-bottom: 0px;margin-top: 10px;" href="#!" onclick="showAllNews(' + numNewsToShow + ');">Leggi Tutto</a><p style="top: 0px;left: 35px;">(' + (jQuery('.sNew').length - numNewsToShow) + ' articoli)</p></div> </div>').insertAfter(jQuery('.tagsContainer')[numNewsToShow]);
            }
            /*});        */
        }

        if (jQuery('div.new-dockable-menu:visible').length > 0)
            new SimpleBar(jQuery('.scrollable-widgets')[0]);
    });

    function showAllDocs(numDocToShow) {
        jQuery(jQuery('.sNewDoc')[numDocToShow]).nextAll(jQuery('.sNewDoc')).show();
        jQuery('.readmored').hide();
        jQuery(jQuery('.sNewDoc')[numDocToShow])[0].style.marginBottom = "5px";
        jQuery(jQuery('.sNewDoc')[numDocToShow + 2])[0].style.marginBottom = "100px";
        return false;
    }

    function showAllNews(numNewsToShow) {
        jQuery(jQuery('.sNew')[numNewsToShow]).nextAll(jQuery('.sNew')).show();
        jQuery(jQuery('.sNew')[numNewsToShow])[0].style.marginBottom = "0px";

        if (window.matchMedia("(min-width: 641px)").matches == true) {
            jQuery(jQuery('.sNew')[numNewsToShow]).next(jQuery('.tagsContainer')).show();
        } else {
            jQuery(jQuery('.sNew')[numNewsToShow]).nextAll(jQuery('.tagsContainer').hide());
            jQuery('.content-wrap')[0].style.marginBottom = "60px";
        }

        jQuery('.readmored').hide();
        return false;
    }

</script>

    <script>
        jQuery(".header-dockable-right").hide();
    </script>


