                                <script>
                                    // Load this script immediately so it is available in the DOM without waiting for
                                    // external dependencies.
                                    function setFlyOutMouseState(state) { window.mouseState = state; }
                                    function handleFlyOutSubPane(string, action) {
                                        switch (action) {
                                            case 'show':
                                                if (typeof jQuery === 'undefined') {
                                                    var element = document.querySelector('li[data-srcid="' + string + '"]');
                                                    var subList = document.querySelector('ul[data-srcid="' + string + '-block"]');
                                                    var preview = document.querySelector('img[data-srcid="' + string + '-image"]');
                                                    element.style.display = 'block';
                                                    preview.style.display = 'inline-block';
                                                    subList.style.display = 'inline-block';
                                                    element.parentNode.style.display = 'block';
                                                } else {
                                                    $('ul.subpane:not(.always-show) li').hide();
                                                    $('ul.subpane').show();
                                                    $('ul.subpane li[data-srcid="' + string + '"]').show();
                                                    $('ul.subpane ul[data-srcid="' + string + '-block"]').show();
                                                    $('ul.subpane ul[data-srcid="' + string + '-block"] li').show();
                                                    $('ul.subpane img[data-srcid="' + string + '-image"]').show();
                                                }
                                                break;
                                            case 'hide':
                                                if (window.mouseState && window.mouseState === 'inside') {
                                                    return;
                                                } else {
                                                    var element = document.querySelector('li[data-srcid="' + string + '"]');
                                                    var subList = document.querySelector('ul[data-srcid="' + string + '-block"]');
                                                    var preview = document.querySelector('img[data-srcid="' + string + '-image"]');
                                                    element.style.display = 'none';
                                                    preview.style.display = 'none';
                                                    subList.style.display = 'none';
                                                    element.parentNode.style.display = 'none';
                                                }
                                                break;
                                        }
                                    }
                                </script>
