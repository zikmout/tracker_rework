<!--  AddThis -->
<script type="text/javascript">
	var firstImageOfComponent = undefined;
	function getMainComponentFirstImage(){
		try{
	    var addthisInstance = document.getElementsByClassName("addthisheader")[0];
	    if(addthisInstance!==undefined){
	    	var component = addthisInstance.parentNode.parentNode.parentNode;
	    	if(component!==undefined){
	    		var firstImg = component.getElementsByTagName("img")[0];
	    		if(firstImg !== undefined){
	    			firstImageOfComponent = firstImg.getAttribute("src");
	    			if(firstImageOfComponent.lastIndexOf("http", 0) !== 0){
	    				var url = window.location.href;
	    				var arr = url.split("/");
	    				firstImageOfComponent = arr[0] + "//" + arr[2] + firstImageOfComponent;
	    			}
	    			// on ajoute la balise meta og pour facebook
	    			var metaOG = document.createElement('meta');
	    			metaOG.setAttribute("property","og:image");
	    			metaOG.setAttribute("content", firstImageOfComponent);
	    			if (typeof metaOG!="undefined")
	    		    document.getElementsByTagName("head")[0].appendChild(metaOG);
	    			//console.log('-- javascript.jsp -- found first image of main component for addthis facebook',firstImageOfComponent);
	    		}
	    	}
	    }
	    if(firstImageOfComponent===undefined){
	    	//console.log('-- javascript.jsp -- found  no first image of main component for addthis facebook');
	    }
	  }catch(err){
	    console.error('-- javascript.jsp -- Error getting first image of main component for addthis facebook inclusion',err);
	  }
	};
	//console.log('-- javascript.jsp -- Search for the first image of the main component...');
	getMainComponentFirstImage();
	var addthis_config = {
	  ui_language: "en"
	};
	var addthis_share = addthis_share || {};
	addthis_share = {
	  passthrough : {
	    twitter: {
	    	via: "Arkema_group"
	    },
	    facebook : {
	    	media : firstImageOfComponent
	    }
	  }
	};
</script>

