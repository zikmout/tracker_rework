  <div class="content">
    <div  about="/en/shareholders-and-investors/relevant-events" typeof="sioc:Item foaf:Document" class="ree_node_default node node-page node-even view-mode-full       clearfix">

  



	                <div  class="group-top  ">
                        
        <div class="field field-name-title field-type-ds field-label-hidden">
            <div class="field-items">
                <div class="field-item even" property="dc:title">
                    <h2>Relevant events</h2>
                </div>
            </div>
        </div>            <div id="block-ree-custom-navigation4--4" class="block block-ree-custom">

	  		  

  <div class="content">
    <a name="main-menu" title="Navigation"></a>  </div>
</div>
 
            
          <div class="field field-name-field-page-subtitle field-type-text-long field-label-hidden">
              <div class="field-items">
                  <div class="field-item eve">
                      Significant events and other communications from Red Eléctrica Corporación, S.A. to the Comisión Nacional del Mercado de Valores (CNMV).
                  </div>
              </div>
          </div>            </div>
    





<div  class="group-inner   ">
