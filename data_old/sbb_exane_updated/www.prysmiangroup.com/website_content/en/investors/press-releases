<div id="block-search-mobile" class="prys2-ddmenu-content">
<div class="container-fluid sub-nav">
<div class="row sub-nav-blocks-container">
<div class="col-xs-12 ddmenu-body">

<form id="prys2-mobile-search-form" class="prys2-search-form" accept-charset="UTF-8" method="post">
<div class="form-group">
<div class="input-group">
<input name="keys" id="search-input-mobile" type="text" class="search-input-text" placeholder="Search..." class="form-control">
<div class="input-group-addon">
<button type="submit" class="btn-icon" onclick="" style="">
<i class="glyphicon glyphicon-search" aria-hidden="true"></i>
</button>
</div>
</div>
</div>
</form>

