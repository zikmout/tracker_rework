                var valorSite = "&site=".concat(encodeURIComponent(ParseSites()));
                var valorSinFiltro = "&filter=0";
                */

                window.location.href = '/en/buscador.cshtml'.concat("?").concat(valorQuery);//.concat(valorSort).concat(valorGetFields).concat(valorOutput).concat(valorClient).concat(valorSite).concat(valorSinFiltro);
            } else {

                   window.location.href = '/en/buscador.cshtml';
