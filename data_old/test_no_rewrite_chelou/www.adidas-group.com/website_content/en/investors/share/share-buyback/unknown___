<section class="  responsive-table-old " data-funct="responsivetable" data-plugin-uid="Eu4Fr" role="responsivetable" data-inited="true">
    
    <h2>May</h2>

</section>


<div class="accordion" data-funct="accordion" data-plugin-uid="zdNpI" role="accordion" data-inited="true">
    <div class="panel" role="tabpanel">
        <section class="headline">
            <h2 class="center-vertical clickable">
                <a href="#/2014/week-may-11-may-15-2015/" data-level="1" data-funct="deeplink" data-funct-options="'closable': true" data-name="/2014/week-may-11-may-15-2015/" data-plugin-uid="lmqmu" role="deeplink" data-inited="true"><i class="icon-expansion-arrow"></i>Week May 11 - May 15, 2015</a>
            </h2>
        </section>
        <div class="accordion-wrapper" style="display: none;">




<section class="  responsive-table-old " data-funct="responsivetable" data-plugin-uid="UOM1S" role="responsivetable" data-inited="true">
    
    <div class="table-container"><div class="table-wrapper"><table style="font-family: DINCompPro, Helvetica, sans-serif; font-style: normal; font-variant: normal;" role="table" cellspacing="0" cellpadding="0" border="0">
	<tbody style="font-size: 12px;">
		<tr style="font-size: 12px;">
			<td style="font-size: 12px;"><span style="font-family: DINCompPro-Bold, Helvetica, sans-serif; font-size: 12px;"><span style="line-height: 19px; font-size: 12px;">Date</span></span></td>
