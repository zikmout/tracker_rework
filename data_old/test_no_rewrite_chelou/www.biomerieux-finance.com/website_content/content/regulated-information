                    jQuery(document).ready(function() {
                        if(jQuery('div.video-player').length){
                            jQuery('div.video-player').each(function(){
                                var id = jQuery(this).attr('id');
                                var video_url = jQuery(this).data('file');
                                var video_image = jQuery(this).data('image');
                                var video_width = jQuery(this).data('width');
                                var video_height = jQuery(this).data('height');

                                var jwconf = {
                                    file: video_url,
                                    skin: '/sites/all/libraries/jwplayer/skins/newtubedark/newtubedark.zip',
                                    modes: [
                                        {type: 'html5'},
                                        {type: 'flash', src: '/sites/all/libraries/jwplayer/player.swf'},
                                        {type: 'download'}
                                    ]
                                    ,image: video_image
                                };

                                if (video_height && video_width) {
                                    jwconf.height = video_height;
                                    jwconf.width = video_width;
                                }

                                jwplayer(id).setup(jwconf);

                                if(jQuery.browser.msie && parseInt(jQuery.browser.version)<9 ){
                                    jwplayer(id).onBeforePlay(function(){
                                        document.title = 'Investors | bioMérieux Corporate Website';
                                    });
                                    document.title = 'Investors | bioMérieux Corporate Website';
                                }
                            });
                        }
                    });
	 
