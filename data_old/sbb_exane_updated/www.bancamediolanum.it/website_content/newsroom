        document.write('<script src="/static-assets/js/plugins/bm-banner.js"><\/script>');
    }
    if (jQuery(".check_BoxCommitments").length > 0) {
        document.write('<script src="/static-assets/js/masonry.min.js"><\/script>');
    }


    if (jQuery(".check_BoxProdottoAD").length > 0) {

        jQuery(document).ready(function () {
            if (jQuery(".minibox-item").find(".text2button").length > 0) {
                jQuery(".minibox-item").find(".text2button").hide();
                /* jQuery( ".minibox-item" ).find( "h3" ).hide(); */
            }


            jQuery(".minibox-item").each(function () {
                if (jQuery(this).find("h3").length > 1) {
                    jQuery(this).find("h3").last().hide();
                }
                if (jQuery(this).find("p").length > 1) {
                    jQuery(this).find("p").last().hide();
                }
            });

        });

        jQuery(window).load(function () {
            if (jQuery('.module.minibox-area > h2')[0].textContent == "") {
                jQuery('.module.minibox-area > h2').hide();
            }

            if (jQuery('.subTitleAD')[0].textContent == "") {
                jQuery('.subTitleAD').hide();
            }
        });
    }

    if (jQuery("h1 > a > span").length > 0) {
        jQuery("h1 > a > span")[0].style.fontSize = "24px";
    }
    if (jQuery("h3 > a > span").length > 0) {
        jQuery("h3 > a > span")[0].style.fontSize = "24px";
    }


</script>
<script type="text/javascript">
    (function ($) {
        $(function () {
            $('[name="desktopSearchText"]').on("focus", function () {
                var hDockSel = 'div.header-dockable-top';
                /*if (!$(hDockSel).hasClass('docked')) {
                    $("body,html").animate({scrollTop : $(hDockSel).position().top});
                }*/
                if (document.getElementById("desktopSearchText").value.length >= 3) {
                    $(".search-suggest").stop(true, true).fadeIn({queue: false}).css('display', 'none').slideDown();
                }
            }).on("blur", function () {
                $(".search-suggest").stop(true, true).fadeOut({queue: false}).slideUp();
            });
        });
    })(jQuery);
</script>

