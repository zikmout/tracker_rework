<script>
	/* Gestione recupero pratica */

	var urlprefix = '/ag/ad/jb/bmed-fe-jbus-accessodiretto-web/rest';
	var link = document.location.protocol + '//' + document.location.hostname;
	if (document.location.port) {
		link += ':' + document.location.port;
	}

	function openRP() {
		var contentH, cOverlay = jQuery('.ovl-main-container.recpra');
		cOverlay.show();
		contentH = cOverlay.outerHeight(true) + cOverlay.position().top;
		cOverlay.hide();
		jQuery('.overlay').css({
			height: Math.max(jQuery(document).height(), contentH),
			width: jQuery(document).width()
		}).fadeIn();

		cOverlay.fadeIn();

		/* reset forms */
		hideAllForms();
		jQuery('#form-init-rp').show();
		jQuery('#rp-un').focus();

		jQuery('#rp-un, #rp-pw').keypress(function (e) {
			if (e.which == 13) {
				login();
				return false;
			}
		});
		jQuery('#rp-cf').keypress(function (e) {
			if (e.which == 13) {
				forgotUserS();
				return false;
			}
		});

		return false;
	}

	function hideAllForms() {
		jQuery('#form-init-rp').hide();
		jQuery('#form-init-rp-ko').hide();
		jQuery('#rp-un').val('');
		jQuery('#rp-pw').val('');
		jQuery('#form-forgotun-rp').hide();
		jQuery('#rp-cf').val('');
		jQuery('#form-forgotun-rp-ok').hide();
		jQuery('#form-forgotun-rp-ko').hide();
		jQuery('#form-forgotpw-rp').hide();
		jQuery('#rp-cf-pw').val('');
		jQuery('#form-forgotpw-rp-ok').hide();
		jQuery('#form-forgotpw-rp-ko').hide();
	}

	function login() {
		jQuery('#rp-un').css('border-color', 'initial');
		jQuery('#rp-pw').css('border-color', 'initial');

		var un = jQuery.trim(jQuery('#rp-un').val());
		var pw = jQuery.trim(jQuery('#rp-pw').val());
		if (un.length == 0) {
			jQuery('#rp-un').css('border-color', 'red');
			return;
		}
		if (pw.length == 0) {
			jQuery('#rp-pw').css('border-color', 'red');
			return;
		}

		jQuery.ajax({
			url: urlprefix + '/diracc/owner/login',
			type: 'POST',
			contentType: 'application/json',
			dataType: 'json',
			data: JSON.stringify({
				n: 0,
				login: un,
				password: pw
			}),
			success: function (data) {
				hideAllForms();
				var res = data.errorCode;
				if(res != undefined && res == 'SUCC') {
					window.location.href = '/accesso-diretto/myfreedomone#/Step/AreaPreventivi';
				} else {
					jQuery('#form-init-rp-ko').show();
				}
			},
			error: function (data) {
				hideAllForms();
				jQuery('#form-init-rp-ko').show();
			}			
		});

		return false;
	}

	function backtologin() {
		hideAllForms();
		jQuery('#form-init-rp').show();
		return false;
	}

	function forgotUser() {
		hideAllForms();
		jQuery('#form-forgotun-rp').show();
		jQuery('#rp-cf').focus();
		return false;
	}

	function forgotUserS() {
		jQuery('#rp-cf').css('border-color', 'initial');
		var cf = jQuery('#rp-cf').val();
		if (cf.length == 0) {
			jQuery('#rp-cf').css('border-color', 'red');
			return;
		}
		jQuery.ajax({
			url: urlprefix + '/diracc/recovery/username',
			type: 'POST',
			contentType: 'application/json',
			data: JSON.stringify({
				codicefiscale: cf
			}),
			success: function (data) {
				hideAllForms();
				var res = data.errorCode;
				if(res != undefined && res == 'SUCC') {
					jQuery('#form-forgotun-rp-ok').show();
				} else {
					jQuery('#form-forgotun-rp-ko').show();
				}
			},
			error: function (data) {
				hideAllForms();
				jQuery('#form-forgotun-rp-ko').show();
			}
		});
		return false;
	}

	function forgotPwd() {
		hideAllForms();
		jQuery('#form-forgotpw-rp').show();
		jQuery('#rp-cf-pw').focus();
		return false;
	}

	function forgotPwdS() {
		jQuery('#rp-cf-pw').css('border-color', 'initial');
		var cf = jQuery('#rp-cf-pw').val();
		if (cf.length == 0) {
			jQuery('#rp-cf-pw').css('border-color', 'red');
			return;
		}

		jQuery.ajax({
			url: urlprefix + '/diracc/recovery/password',
			type: 'POST',
			contentType: 'application/json',
			data: JSON.stringify({
				codicefiscale: cf,
				link: link
			}),
			success: function (data) {
				hideAllForms();
				var res = data.errorCode;
				if(res != undefined && res == 'SUCC') {
					jQuery('#form-forgotpw-rp-ok').show();
				} else {
					jQuery('#form-forgotpw-rp-ko').show();
				}
			},
			error: function (data) {
				hideAllForms();
				jQuery('#form-forgotpw-rp-ko').show();
			}
		});

		return false;
	}
</script>
