      <h1 class="section-title col-xs-20 col-sm-15">
        Regulated information      </h1>
    </div>

    <div class="node node_article col-xs-20 col-sm-15">
      <div class="inner-content container-fluid">
        <!--      Teaser-->
        
        <!--      Stock chart-->
        
        <!--      Sommaire-->
        
                              <div class="item clearfix item-1 no-case">
                                            <div class="field field-name-field-body field-type-text-with-summary field-label-hidden"><div class="field-items"><div class="field-item even"><style type="text/css">.panel-group .panel-default > .panel-heading h4 {font:200 1.5em raleway !important;}
