  if(typeof webtrekk != "undefined") {
    webtrekk.customParameter[5] = "Standard text"
  }
</script><div  class="container-box  container-box--no-border">
<div class="container-box__inner ">
<h2 class="row-headline row-headline--with-border">
            <span >
              Announcement pursuant to Article 5 <span class="rtlEntity">‏</span>(1b) and <span class="rtlEntity">‏</span>(3) of Regulation <span class="rtlEntity">‏</span>(EU) No. 596/2014</span>
      </h2>
    <div class="row  forced-height  ">
    <div class="col-tn-12 col-xs-6 col-sm-6 col-md-3 col-lg-3 bab-stripe">
              <div class="download teaser light-gray">
