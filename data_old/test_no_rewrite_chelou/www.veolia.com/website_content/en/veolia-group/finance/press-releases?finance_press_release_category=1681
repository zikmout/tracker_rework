<article role="article" about="/en/veolia-group/finance/financial-information/press-releases" class="readspeaker-content">
  
  
  <div>
    
      <div>
              <div>  <div class="paragraph paragraph--type--listing paragraph--view-mode--default">
          
            <div><div class="views-element-container">





<div  class="listing--listing-node-finance-press-release js-view-dom-id-fed97d81219d45c3b3c248111f335bfe71829268259a7d3169e4033d622d1651 listing">
  
    

      <div class="container container--filter container--form--views-exposed-form container--width--standard container--space-inner--v-m rs_skip  theme--background-color--veo-gray">
      <div class="container__inner">
          

<form  class="views-exposed-form form--views-exposed-form filter" data-drupal-selector="views-exposed-form-listing-node-finance-press-release-block-listing-node-finance-press-release" action="/en/veolia-group/finance/financial-information/press-releases" method="get" id="views-exposed-form-listing-node-finance-press-release-block-listing-node-finance-press-release" accept-charset="UTF-8" target="_self">
  <div class="form-grid form-grid--align--bottom">
                    <div class="form-col-3 form-col-12-m">
          



<div  class="js-form-item form-item js-form-type-select form-item-select form-item-finance_press_release_category js-form-item-finance_press_release_category form-no-label">
  
