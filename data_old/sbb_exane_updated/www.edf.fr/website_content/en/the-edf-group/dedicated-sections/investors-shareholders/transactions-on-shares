<script type="text/javascript">
<!--//--><![CDATA[//><!--

(function($) {
	$(document).ready(function() {
		var $input = $("input[name=keywords]"),
			setupAC = function() {
				$input.autocomplete({
					source: function(request, response) {
						var results = $.ui.autocomplete.filter(myArr, request.term);
							response(results.slice(0, nbRes));
					},
					minLength: 3,
					select: function(event, ui) {
						$input.val(ui.item.value)
							.closest("form").submit();
					}
				}).autocomplete("option", "messages", {
					/* noResults: "No search results.",*/
					noResults : "Aucun résultat disponible",
					results : function(t) {
						/* return t + (t > 1 ? " results are" : " result is") + " available, use up and down arrow keys to navigate." */
						return t + (t > 1 ? " résultats sont disponibles" : " résultat est disponible") + ", utiliser les touches haut et bas pour naviguer.";
					}
				});
			},
			myArr = [],
			url = "//www.synomia-autocompletion.com/suggest_data.php?mid=ec05647b891fe72b290ce7d0095daefa&format=jquery",
			nbRes = 10;

		$input.focus(function() {
			if ("undefined" === typeof this.suggestalreadyLoaded) {
				$.getJSON(url, function(data) {
					myArr = data;
					setupAC();
				});
			}
			this.suggestalreadyLoaded=true;
		});

		if ($.ui && $.ui.autocomplete) {
			$.ui.autocomplete.filter = function (array, term) {
				var matcher = new RegExp("^" + $.ui.autocomplete.escapeRegex(term), "i");
				return $.grep(array, function (value) {
					return matcher.test(value.label || value.value || value);
				});
			};
	    }
	});
})(jQuery);

//--><!]]>
</script>
