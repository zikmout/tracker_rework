</select>
<script type="text/javascript">
					//<![CDATA[
					var urls_1 = {"en":"https:\/\/newsroom.st.com\/","it":"https:\/\/newsroom.st.com\/it\/newsroom-it","fr":"https:\/\/newsroom.st.com\/fr\/newsroom-fr","zh":"https:\/\/newsroom.st.com\/zh\/newsroom","ja":"https:\/\/newsroom.st.com\/ja\/newsroom"};
					document.getElementById( "lang_choice_1" ).onchange = function() {
						location.href = urls_1[this.value];
					}
					//]]>
				</script>                    </div>
                </li>
            </ul>
            <div class="st-header-search-mobile">
                <svg class="st-svg--search-icon">
                    <use href="https://newsroom.st.com/wp-content/themes/st-news/icons/st-icons.svg#icon-search"></use>
                </svg>
            </div>

            <script>
    (function(){

        var form = $("form.st-search").get(0);
        var dropdownTrigger = $(form).find(".st-search__select").get(0);
        var dropdown = $(form).find(".st-search__dropdown").get(0);
        var searchHistory = $(form).find(".st-search__searchlist").get(0);
        var searchResults = $(form).find(".st-search__searchlist").get(1);
        var searchInput = $(form).find("input.st-search__input").get(0);
        var searchButton = $(form).find(".st-search__button").get(0);
        var timeoutID = 0;
        var searchCategory = "products";
        
        var searchMobileComponent = document.getElementsByClassName('st-search--mobile')[0];
        var formMobile = $("form.st-search__form-mobile").get(0);
        var searchInputMobile = $(formMobile).find("input.st-search__input").get(0);
        var searchButtonMobile = $(formMobile).find("a.st-button.st-button--primary").get(0);

        var searchHistoryMobile = $(searchMobileComponent).find(".st-search__searchlist").get(0);
        var searchResultsMobile = $(searchMobileComponent).find(".st-search__searchlist").get(1).getElementsByTagName('div')[0];
        
        var hintsCurrentContainer = searchResults;
        var historyCurrentContainer = searchHistory;
        var currentSearchInput = searchInput;
        
        
        function toggleElement(element, state){
            if(state == undefined){
                state = !($(element).hasClass("open")) || $(element).hasClass("hidden");
            }
            if(state){
                $(element).addClass("open");
                $(element).removeClass("hidden");
            }else{
                $(element).removeClass("open");
                $(element).addClass("hidden");
            }
        }

        function createProductsEntry(label, value){
            var p = document.createElement('p');
            p.className='st-copy';
            p.innerHTML='<a href="https://st.com' + value + '">' + label + '</a>';
            return p;
        }

        function createKeywordsEntry(label, value){
            var p = document.createElement('p');
            p.className='st-copy';
            if(value.includes("http", 0)){
                p.innerHTML='<a href="' + value + '" class="autosuggestion-black-font">' + label + '</a>';
            }else{
                p.innerHTML='<a href="https://st.com' + value + '" class="autosuggestion-black-font">' + label + '</a>';
            }
            return p;
        }
        
        function createSectionTitle(title_value){
            var title = document.createElement('h5');
            title.className="st-h5";
            title.innerHTML = title_value;
            return title;
        }

        function fillHints(data){
            var products = [];
            var keywords = [];
            for(var i=0; i < data.length; i++){
                var label = data[i].value;
                if(data[i].data.keywords){
                    keywords.push(createKeywordsEntry(label, data[i].data.keywords));
                }
                if(data[i].data.products){
                    products.push(createProductsEntry(label, data[i].data.products));
                }
            }
            hintsCurrentContainer.innerHTML = "";
            if(products.length){
                hintsCurrentContainer.appendChild(createSectionTitle('Products'));
                for(var i=0; i < products.length; i++){
                    hintsCurrentContainer.appendChild(products[i]);
                }
            }
            if(keywords.length){
                hintsCurrentContainer.appendChild(createSectionTitle('Keywords'));
                for(var i=0; i < keywords.length; i++){
                    hintsCurrentContainer.appendChild(keywords[i]);
                }
            }
        }

        function composeHintSearchRequest(){
            var URLBase = "https://newsroom.st.com/bin/st/search-cx/searchautosuggestion";
            var fullRequest = URLBase + '?q=' + currentSearchInput.value;
               fullRequest +="&category=products&productsNo=5&keywordsNo=5&isSearchWidget=1";
               fullRequest +="&lang=en";
               fullRequest +="&rand=" + new Date().getTime();
            return fullRequest;
        }

        function composeSearchRequest(inputValue){
            var URLBase = "https://www.st.com/content/st_com/en/search.html";
            var fullRequest = URLBase + '?widgetSearchBar=' + inputValue;
                fullRequest += '&widgetSearchAction=1&_charset_=UTF-8' + 
                                '&filter-search=' + searchCategory +
                                '&%3Acq_csrf_token=undefined';
                return fullRequest;
        }

        function requestHints(){
            if(currentSearchInput.value.length<3){
                return;
            }
            $.ajax({
                url: composeHintSearchRequest(),
                dataType: "json",
                data: 'json',
                success: function(data){
                    if(typeof data!="object"){
                        data = JSON.parse(data);
                    }
                    fillHints(data);
                    toggleElement(hintsCurrentContainer, true);
                    toggleElement(historyCurrentContainer, false);
                }
            });
        }

        function triggerrequest(){
            clearTimeout(timeoutID);
            timeoutID = setTimeout(function(){
                requestHints();
            },500);
        }

        //events
        $("input[name=filter-search]").on("click", function(){
            searchCategory = this.value;
           var label = $("label[for='" + $(this).attr('id') + "']").html();
            $(dropdownTrigger).find('span').html(label);
        });

        $(dropdownTrigger).on("click", function(e){
            toggleElement(dropdown);
            e.stopPropagation();
        });

        $(searchInput).keyup(function(e){
            var value = this.value;
            currentSearchInput = this;
            searchButton.setAttribute('href', composeSearchRequest(value));
            hintsCurrentContainer = searchResults;
            historyCurrentContainer = searchHistory;
            if(value.length < 3){
                toggleElement(hintsCurrentContainer, false);
                toggleElement(historyCurrentContainer, true);
                return false;
            }
            
            triggerrequest();
        });

        $(searchInputMobile).keyup(function(e){
            var value = this.value;
            currentSearchInput = this;
            hintsCurrentContainer = searchResultsMobile;
            historyCurrentContainer = searchHistoryMobile;
            searchButtonMobile.setAttribute('href', composeSearchRequest(value));
            if(value.length < 3){
                return false;
            }
           
            toggleElement(hintsCurrentContainer, false);
            toggleElement(historyCurrentContainer, true);
            triggerrequest();
        });



        $(searchInput).keypress(function(e){
            if(e.keyCode == 13){
                e.stopPropagation();
                searchButton.click();
                return false;
            }
           
        });

        $(searchInputMobile).keypress(function(e){
            if(e.keyCode == 13){
                e.stopPropagation();
                searchButtonMobile.click();
                return false;
            }
           
        });

        $(searchInput).on("click", function(e){
            toggleElement(dropdown, true);
            if(searchInput.value.length>2){
                triggerrequest();
            }
            e.stopPropagation();
        });

        $(searchButton).on("click", function(e){
            if(!searchInput.value){ 
               e.preventDefault(); 
               return false;
            }
        });

        $(searchButtonMobile).on("click", function(e){
            if(!searchInputMobile.value){ 
               e.preventDefault(); 
               return false;
            }
        });

        $(document).on('click', function(){
            toggleElement(dropdown, false);
        });
    })();

    (function(){
        var searchMobileButton = document.getElementsByClassName('st-header-search-mobile')[0];
        var searchMobileComponent = document.getElementsByClassName('st-search--mobile')[0];
        var cancelButtonMobile = document.getElementsByClassName("st-search--close")[0];
        
        function toggle(state){
            if(state==undefined){
                state = !$(searchMobileComponent).hasClass('fade-in');
            }
            if(state){
                $(searchMobileComponent).addClass('fade-in');
                $(document.body).addClass('noscroll');
                $(searchMobileButton).find('svg').addClass('toggle');
            }else{
                $(searchMobileComponent).removeClass('fade-in');
                $(document.body).removeClass('noscroll');
                $(searchMobileButton).find('svg').removeClass('toggle');
            }
        }
    
        searchMobileButton.onclick = function(){
            toggle();
        }

        $(cancelButtonMobile).on('click', function(){
            toggle(false);
        });
    })();
    
</script>            <div class="st-hamburger-menu">
                <svg class="st-svg--block st-svg--burger">
                    <use href="https://newsroom.st.com/wp-content/themes/st-news/icons/st-icons.svg#icon-burger"></use>
                </svg>
                <svg class="st-svg--block st-svg--close">
                    <use href="https://newsroom.st.com/wp-content/themes/st-news/icons/st-icons.svg#icon-close"></use>
                </svg>
            </div>

            <script>
                var ignoredLanguages = ['it', 'fr'];
                var localeLan = 'en';
                for (var i = 0; i < ignoredLanguages.length; i++) {
                    var selectOpt = $('.st-lang-select select option[value=' + ignoredLanguages[i] + ']').remove();
                }

                if (localeLan == 'ja') {
                    $( ".st-lang-select select " ).prepend($('.st-lang-select select option[value="ja"]').remove());
                } else if (localeLan == 'zh') {
                    $( ".st-lang-select select " ).prepend($('.st-lang-select select option[value="zh"]').remove());
                } else {
                    $( ".st-lang-select select " ).prepend($('.st-lang-select select option[value="en"]').remove());
                }
            </script>
		</div><!-- .st-header -->

