<div class="desktop-cookie-policy container-fluid " >
    <div class="row flexable">
		<div class="col-xs-11">
			<p><span>This site uses technical, analytical and profiling cookies, own and other sites, to send you advertising and services in line with your preferences. For information and to find out how to refuse consent to their use or how to disable them <a href="/gestione-cookie" target="">click here</a>. By clicking on any point on the screen, scrolling the page or closing this banner, you consent to the use of all cookies.</span></p>
		</div>
		<div class="col-xs-1" id="close-cookie">
			<div class="close-button"></div>
		</div>
    </div>
</div>

<script type="text/javascript">
    $(window).load(function() {
        var containerCookieBanner = $('.desktop-cookie-policy').parent();
        var spanCloseCookieBanner = $('#close-cookie');
        var navbar = $('.navbar.affix-top');
        var content = $('#content');
        var wrapper = $('#wrapper');
        var containerHeight;
        var navbarMargin;
        var active = false;

        $(window).resize(function() {
            if (active) {
                containerHeight = containerCookieBanner.outerHeight();
                if (containerHeight > 0) {
                    navbar.css("top", containerHeight);
                }
            }
        });

        /* FUNCTIONS */
        var closeCookieBanner = function (){
            containerCookieBanner.css("height", 0);
            containerCookieBanner.slideUp();
            active = false;
            navbar.removeClass("cookie");
            content.removeClass("cookie");
            wrapper.removeClass("cookie");
            navbar.css("top", "0");
            setCookie("05387-IST-PRIVACY_READ", "true", 365);
        }

        var showCookieBanner = function (){
            containerCookieBanner.slideDown({
                complete: slideNavbar
            });
            active = true;
        }

        function slideNavbar() {

            containerHeight = containerCookieBanner.outerHeight();
            if (containerHeight > 0) {
                navbar.css("top", containerHeight);
                navbar.addClass("cookie");
                content.addClass("cookie");
                wrapper.addClass("cookie");
            }

        }

        /* EVENTS */
        spanCloseCookieBanner.bind('click',closeCookieBanner);
        containerCookieBanner.bind('blur' ,closeCookieBanner);
        containerCookieBanner.bind('focus',function(event){});

        setTimeout(function(){
            $(window).on('scroll',function(event){
                containerCookieBanner.trigger('blur');
            });
        },1500);

        containerCookieBanner.trigger('focus');

        /* UTILITY COOKIE */
        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(';');
            for(var i = 0; i <ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays*24*60*60*1000));
            var expires = "expires="+ d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=istituzionale.bper.it;path=/";
        }

        var beperCookie = getCookie("05387-IST-PRIVACY_READ");

        if (beperCookie == "") {
            showCookieBanner();
        }

    });
</script>
