<script type="text/javascript">

    var tpl_ultimas_noticias, tpl_noticias, tpl_blog, tpl_ultimos_tweets;
    var pp = 5;
    var categoria_actual = "0", pagina_actual = 1;
   
    var optBlock = { 
     	message: 'Loading',
     	overlayCSS:  { 
	        backgroundColor: '#000', 
	        opacity:         0, 
	        cursor:          'wait',
	        border: 'none'
        },
        css: {  
	        color:          '#0198CF', 
	        border:         '1px solid #CCC', 
	        backgroundColor:'#F0F0F0', 
	        cursor:         'wait',
	        margin: '10px', padding:'10px'
	    }, 
    };
    
    function loadblog() {
        $.ajax({
			dataType: "json",
			url: "https://api.typepad.com/blogs/6a00e00980104088330120a7c2354b970b/post-assets.json?max-results=5",
			cache: true,
			success: function(data){
				$.each( data.entries, function( i, item ) {
					var anyo = item.published.substring(0,4);
					var mes = item.published.substring(5,7);
					var dia = item.published.substring(8,10);
					var datos = {
	            		titulo: item.title.replace("%","%25"),
	            		url: item.permalinkUrl,
	            		categoria: item.categories[0],
	            		fecha: dia + "/" + mes + "/" + anyo
	            	};
					$('#blog').append(tpl_blog(datos));
				});
			}
		});
    }
     

    function loadNoticias(categoria, pagina) {
        //console.log("categoria: " + categoria);

        categoria_actual = categoria;
        pagina_actual = pagina;
        var esTag = "";
        var tag = "";
        console.log("esTag = " + esTag);
        console.log("tag = " + tag);
        console.log("esTag = " + esTag);

        if (esTag != undefined && esTag != null && esTag == "True" && tag != undefined && tag != null) {
            categoria_actual = tag;
        }
        console.log("categoria_actual = " + categoria_actual);
        $('#noticiascategorias').block(optBlock); 
        
        
        $.getJSON("/en/ajax/GetByCategoria/" + categoria_actual, { "numpagina": pagina, "numelementos": pp }, function (data) {
            if (data.totalelementos != 0) {
            	$('#noticiascategorias').html(''); 
            	$.each( data.noticias, function( i, item ) { 
            		var find = '%';
            		var re = new RegExp(find, 'g'); 
	            	var datos = {
	            		tituloClean: item.titulo.replace(re,"%25"),
	            		titulo: item.titulo,
	            		rutaImagen: item.rutaImagen,
	            		URL: item.URL,
	            		nombrecategoria: item.nombrecategoria,
	            		urlcategoria: item.urlcategoria,
	            		fecha: item.fecha,
	            		anyo: item.anyo, 
	            		mes: item.mes,
	            		pie:item.pie,
	            		entradilla: item.entradilla
	            	};
	                 $('#noticiascategorias').append(tpl_noticias(datos));
            	});
               
                generaPaginacion(data.totalelementos, pp, data.pagina);
                		
            }else{
	            
	             $('#noticiascategorias').append('No news in this category.');
            }
            
            $('#noticiascategorias').unblock(); 
        }).fail(function () {
            $('#noticiascategorias').unblock();
            console.log("error");
        });
        
        return false;
    }

    function generaPaginacion(total, pp, p) {
    
        var paginas = Math.ceil(total / pp);
        var optionsPager = {
	        currentPage: pagina_actual,
	        alignment: "center", 
	        totalPages: paginas,
	            onPageClicked: function(e,originalEvent,type,page){
	               loadNoticias(categoria_actual, page);  
	            }
	    } 
	    $('#divPaginacionBoot').bootstrapPaginator(optionsPager);
		
		gapi.plusone.go();  
        FB.XFBML.parse();
        twttr.widgets.load();
        
    }

</script>

    
