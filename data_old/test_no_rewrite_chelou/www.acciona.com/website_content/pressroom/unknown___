<script>
    /* ---------SCRIPT PARA ABRIR PDFS EN VENTANA NUEVA  -----*/
    $("a").each(function () {
        if (this.href.indexOf(".pdf") > 0) {
            this.target = "_blank";
        }
    });



</script>
