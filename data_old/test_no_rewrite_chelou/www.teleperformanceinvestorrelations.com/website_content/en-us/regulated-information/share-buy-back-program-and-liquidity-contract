<script>

		
		
$( document ).ready(function() {	
	$('.download-center').each( function(i) {
		//$('.select').select2();
		
		
		
		var $this        = $(this),
			$yearContent = $this.find('.item'),
			$year        = '',
			$more        = $this.find('.more .submit'),
			$pageSize    = 0,
			$select      = $this.find('.select'),
			$idModulo,
			$qtd         = 10;
		
		$idModulo = $this.find('.controle').text();
		$this.addClass('content-' + i);
		
		$yearContent.unbind('click').click( function( e ) {
			
			e.preventDefault();
			
			$yearContent.removeClass('selected');
			
			$(this).addClass('selected');
			
			$year = $(this).find('span').text();
			
			//$select.select2("val", $year);
			$select.val($year).trigger('change');
			
			if ( $yearContent.length < 7 ) {
				
				window.setTimeout( function() { $('.download-center .next').addClass('disabled'); }, 500);
				
			}
			
			MakeURL( $year, $qtd, $idModulo, i );
			
		});
		
		$this.find('.item:first-child').click();
		
		$select.on("change", function( e ) {
			
			//$year = $select.select2("val");
			
			$year = $select.val();
			
			$yearContent.removeClass('selected');
			
			$yearContent.each( function() {
				
				if( $(this).find('span').text() === $year ) {
					
					$(this).addClass('selected');
					
				}
				
			});
			
			MakeURL( $year, $qtd, $idModulo, i );
			
		});
		
		$more.unbind('click').click( function( e ) {
			
			e.preventDefault();
			
			More( $idModulo, i );
			
		});
		
	});	
});
		
		
			function FixMultiSlide () {
                
				// ainda dou uma paulada no espertinho que usou esse plugin descuidadamente e cagou a porra toda
                $('.cycle-slideshow').each(function () {
                    var that = $(this),
                        offset = parseInt(that.find('.cycle-carousel-wrap').css('left').replace("px", "")) * -1,
                        total = that.find(".cycle-carousel-wrap > div.item").length,
                        visible = parseInt(that.data('cycle-carousel-visible')),
                        rem = total - visible;

                    if (total > that.data('cycle-carousel-visible') && offset < rem * 142) {
                        $(that.data('cycle-next')).removeClass('disabled');
                    }
                });
            }

            function MakeURL ( year, pageSize, idModulo, indice ) {

                var $idModulo = idModulo,
                    $year = year,
                    $pageSize = pageSize,
                    $url      = '/system/ajax/?op=downloadcentral&moduleId=' + $idModulo + '&year=' + $year + '&page=1&pageSize=' + $pageSize;
                    
                    DownloadsCenterAjax( $url, $pageSize, indice );

            }

            function DownloadsCenterAjax ( url, pageSize, indice ) {

                var $more = $('.download-center.content-'+ indice +' .more .submit');

                var $request = $.ajax({
                    url: url,
                    type: "post",

                    beforeSend: function(){

                        $('.download-center.content-'+ indice +' tbody').html('<tr><td colspan="4"><div class="margin-auto-center"><img class="loader-center" src="/img/teleperformance-loader.gif" alt=""></div></td></tr>');

                    },

                    success: function( data ) {

                        if( data ) {

                            // console.log( data.count );
                            $more.removeClass('hide');

                            if( pageSize >= data.count ) {

                                $more.addClass('hide');

                            }

                            MakeRows(data, indice);
                        }

                    },

                    error: function() {

                        console.log('ajax fail - Downloads Center');

                        var $message = $('.download-center.content-'+ indice +' .not-found-message').text();

                        $('.download-center.content-'+ indice +' tbody').html('<tr><td colspan="4">'+ $message +'</td></tr>');

                        $more.addClass('hide');

                        setTimeout(function () {
                            FixMultiSlide();
                        }, 1000);
                    }
                    
                });

            }

            function MakeRows ( data, indice ) {

                var $i,
                    $tr = new Array(),
                    $data = data.list,
                    $list,
                    $title,
                    $type,
                    $size,
                    $link;

                for ( $i = 0; $i < $data.length; $i ++ ) {

                    $title = $data[ $i ].title.replace("'"," ");

                    $type = $data[ $i ].type;

                    $size = $data[ $i ].size;
                    
                    $link = $data[ $i ].link;
					
					
					$v = "event";
					$s = "send";
					$aj = "Download Central";
					$act = "Download File";

                    $list = '<tr class=\''+($i%2!=0?'odd':'')+'\'>';
                    $list = $list + '<td class="txt-left">' + $title + '</td>';
                    $list = $list + '<td>' + $type + '</td>';
                    $list = $list + '<td>' + $size + '</td>';
                    $list = $list + '<td><a href="' + $link + '" class="decorated download-central-links" target="_blank" onclick="javascript:ga(\''+ $s +'\', \''+$v+'\', \''+$aj+'\', \''+$title+'\', \''+ $act +'\');">Download</a></td>';
                    $list = $list + '</tr>';

                    $tr.push( $list );
                }

                setTimeout(function () {
                    $('.download-center.content-' + indice + ' tbody').html($tr.join(''));
                    FixMultiSlide();
					/*
                    $(".download-central-links").click(function () {
                        var data = $(this).data();
                        g.App.Ga.TrackEvent(data['event'], data['action'], data['label']);
                    });
					*/
                }, 1000);
                
            }

            function More ( idModulo, indice ) {

                var $index = 0,
                    $pageSize,
                    $year,
                    $maxItems = $('.download-center.content-'+ indice +' .max-items');

                $('.download-center.content-'+ indice +' tbody tr').each( function(i) {

                    $index = i + 1;

                });

                $pageSize = $index + 5;
				var sizeWindow = $(window).width();   

                if( sizeWindow > 790 ) {

                    $year = $('.download-center.content-'+ indice +' .cycle-carousel-wrap .selected span').html();

                }

                MakeURL( $year, $pageSize, idModulo, indice );

            }		
		
</script>
	
