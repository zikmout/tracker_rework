	var dataTableJS = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/jquery.dataTables.js?currentUri=/en/investor-relations/arkema-share/share-buybacks/2020/index.html&currentSiteRoot=/sites/global&locale=en";
	var columnFilterJS = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/jquery.dataTables.columnFilter.js?currentUri=/en/investor-relations/arkema-share/share-buybacks/2020/index.html&currentSiteRoot=/sites/global&locale=en";
	var dataTableResponsiveJS = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/dataTables.responsive.js?currentUri=/en/investor-relations/arkema-share/share-buybacks/2020/index.html&currentSiteRoot=/sites/global&locale=en";
	var dataTableActivation = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/dataTableMode.js";
	var productSelectionGuideActivation = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/product-selection-guide-manager.js";
	var productSelectionGuidePimActivation = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/product-selection-guide-manager-pim.js";
	var select2Activation = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/libs/select2-4.0.3.full.js";
	var select2Activation2 = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/libs/select2.multi-checkboxes.js";
	var highchartsActivation1 = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/libs/highcharts-5.0.9.min.js";
	var highchartsActivation2 = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/libs/exporting-5.0.9.min.js";
	var flexsliderActivation = "/system/modules/com.arkema.galaxy.opencms.core/resources/js/libs/jquery.flexslider.2.5.js";
