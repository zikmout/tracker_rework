    <script>
      function getCookie(name) {
        var matches = document.cookie.match(new RegExp(
                "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
        ));
        return matches ? decodeURIComponent(matches[1]) : undefined;
      }

      var cookieGA = getCookie('_ga');

      dataLayer = [{
                  'env_work': "prod",
                  'env_language': "EN",
                  'env_country': "EN",
                  'page_cat1': "Regulated informations",
                  'page_template': "Regulated informations",
                'cookieGA': cookieGA
      }];
    </script>

